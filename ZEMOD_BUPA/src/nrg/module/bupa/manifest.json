{
    "sap.ui5": {
        "config": {
            "module": {
                "nrg.module.bupa": {
                    "stylesheet": [
                        "asset/css/module.css"
                    ],

                    "resourceBundle": {
                        "comp-i18n-bupa": "i18n/module.properties"
                    },

                    "odata": {
                        "real": {
                            "comp-bupa": {
                                "url": "sap/opu/odata/sap/ZE_CCUX_SRV/"
                            }
                        },
                        "mock": {
                            "comp-bupa": {
                                "mockDataBaseUrl": "data/devtest/",
                                "generateMissingMockData": true
                            }
                        }
                    }
                }
            }
        },

        "routing": {
            "routes": {
                "bupa.bpInfo": {
                    "pattern": "bupa/bpinfo/bp/{bpNum}/ca/{caNum}",
                    "target":["bupa.BupaBpInfo", "dashboard.SummaryBP", "bupa.Tools"]
                },
                "bupa.caInfo": {
                    "pattern": "bupa/cainfo/bp/{bpNum}/ca/{caNum}",
                    "target":["bupa.BupaCaInfo", "dashboard.SummaryBP", "bupa.Tools"]
                }
            },

            "targets":{
                "bupa.BupaBpInfo": {
                    "viewName": "nrg.module.bupa.view.CustomerDataBpInfo",
                    "controlId": "idAppGeneral"
                },
                "bupa.BupaCaInfo": {
                    "viewName": "nrg.module.bupa.view.CustomerDataCaInfo",
                    "controlId": "idAppGeneral"
                },
                "bupa.Tools": {
                    "viewName": "nrg.module.bupa.view.Tools",
                    "controlId": "idAppTools"
                }
            }
        }
    }
}

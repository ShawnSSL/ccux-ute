<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="nrg.module.campaign.view.Tools">

    <ute:Tag class="nrgToolsEmpty nrgAppMain-toolsContent nrgAppMain-toolsContent-full">
        <ute:Tag class="nrgCamToolsT" >
            <ute:Tag class="nrgCamToolsTitleT" text="{comp-i18n-campaign>nrgCmpToolsTtl}" />
            <ute:Tag class="nrgCamToolsBtnT nrgCamTools-top" >
                <ute:Button
                    id="idnrgCamToolsBtn-H"
                    text="{comp-i18n-campaign>nrgCmpToolsBtnHis}"
                    enabled="true"
                    press="onHistoryPress" >
                </ute:Button>
                <ute:Button
                    id="idnrgCamToolsBtn-C"
                    text="{comp-i18n-campaign>nrgCmpToolsBtnCan}"
                    enabled="true"
                    class="nrgCamTools-top">
                </ute:Button>
            </ute:Tag>
        </ute:Tag>
    </ute:Tag>
</mvc:View>

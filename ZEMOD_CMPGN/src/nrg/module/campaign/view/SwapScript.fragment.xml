<core:FragmentDefinition
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:c="sap.ui.commons"
    xmlns:core="sap.ui.core"
    xmlns:u="ute.ui.commons"
    xmlns:um="ute.ui.main">
<u:Tag class="nrgCamSwpScr">
    <u:Tag class="nrgCamOvs-Dsc">
            <core:HTML content='&lt;div class=&quot;nrgCamOvs-left nrgCamOvs-top nrgCamOvs-OVW&quot;&gt;{view-campaign>TxtLine}&lt;/div&gt;' id="idnrgSwapDesc">
            </core:HTML>
    </u:Tag>
    <u:Tag class="nrgCamSwpScr-btnTag">
        <u:Button
            text="YES"
            enabled="true"
            class="nrgCamOvs-btn"
            press=".onSwapYes">
        </u:Button>
        <u:Button
            text="NO"
            enabled="true"
            class="nrgCamOvs-btn"
            press=".onSwapNo">
        </u:Button>
        <u:Button
            text="Other Deposit Alternatives"
            enabled="true"
            class="nrgCamOvs-btn"
            press=".onSwapOther">
        </u:Button>
    </u:Tag>
</u:Tag>

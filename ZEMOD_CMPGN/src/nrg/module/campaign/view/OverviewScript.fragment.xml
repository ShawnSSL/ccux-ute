<core:FragmentDefinition
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:c="sap.ui.commons"
    xmlns:core="sap.ui.core"
    xmlns:u="ute.ui.commons"
    xmlns:um="ute.ui.main"
    xmlns:l="sap.ui.commons.layout">
<u:Tag>
    <u:Tag>
        <um:Dropdown id="idnrgCamOvsDdL" class="nrgCamOVSLng-DdL" select=".onOvwLngSelected" >
            <um:DropdownItem key="{view-campaign>TxtLang}" id="idnrgCamOvsLngLtIt" class="nrgCamOVSLng-DdLItem">
                <u:Tag text="{parts:[{path:'view-campaign>TxtLang'}],
                                            formatter:'.formatType'}"/>
            </um:DropdownItem>
        </um:Dropdown>
    </u:Tag>
    <u:Tag class="nrgCamOvs-Dsc">
            <core:HTML content='&lt;div class=&quot;nrgCamOvs-left nrgCamOvs-top nrgCamOvs-OVW&quot;&gt;{view-campaign>TxtLine}&lt;/div&gt;' id="idnrgCamOvsOvTv">
            </core:HTML>
    </u:Tag>
    <u:Tag class="nrgCamOvs-btnTag nrgCamOvs-top">
        <u:Button
            id="idnrgCamOvsBtn-Ok"
            text="Accept"
            enabled="true"
            class="nrgCamOvs-btn"
            press=".onOvsAccept">
        </u:Button>
        <um:Dropdown id="idnrgRejectRsnsOV"
                     select=".onDisposition"
                     placeholder="Non-Disposition Reason"
                     class="nrgCamSSRr-DdL nrgCamSS-left">
            <um:DropdownItem key="{view-campaign>RejectionCode}" class="nrgCamSSRr-DdLItem">
                <u:Tag text="{view-campaign>Description}" />
            </um:DropdownItem>
        </um:Dropdown>
    </u:Tag>
</u:Tag>

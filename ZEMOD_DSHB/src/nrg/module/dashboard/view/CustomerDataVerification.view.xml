<!--Routing Pattern: customerDataVerification/bp/{bpNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.dashboard.view.CustomerDataVerification">

    <ute:Tag class="nrgAppMain-generalContent nrgDashboard-cusDataVerify">
        <l:MatrixLayout
            class="nrgDashboard-cusDataVerify-layout"
            widths="288px,534px,84px" columns="3" width="100%">
            <l:rows>
                <l:MatrixLayoutRow>
                    <l:cells>
                        <!--Area Left (active)-->
                        <l:MatrixLayoutCell class="nrgDashboard-cusDataVerify-l">
                            <ute:Tag class="nrgDashboard-cusDataVerify-l-div">
                                <ute:TextView text="{oDtaVrfyContracts>/ContractStatus}"
                                    class="nrgDashboard-cusDataVerify-l-div-title"/>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <utmain:Label labelFor="tdsp"
                                    text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_TdspLbl}"
                                    class="nrgDashboard-cusDataVerify-l-div-tdspLabl"/>
                                <ute:TextView id="tdsp"
                                    text="{oDtaVrfyContracts>/TdspName}"
                                    class="nrgDashboard-cusDataVerify-l-div-tdspVal"/>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label labelFor="meter_num"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_MtrNum}"
                                                class="nrgDashboard-cusDataVerify-l-div-halflabl"/>
                                            <ute:TextView id="meter_num"
                                                text="{oDtaVrfyContracts>/MeterNumber}"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell
                                            class="{comp-i18n-dashboard>nrgDashboard-cusDataVerify-l-div-cell}">
                                            <utmain:Label labelFor="desp_txt"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_Desc}"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="desp_txt"
                                                text="{oDtaVrfyContracts>/MeterDescription}"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:Tag class="nrgDashboard-cusDataVerify-l-div-chb">
                                    <utmain:CheckBox id="chk_advMtr"
                                        class="nrgDashboard-cusDataVerify-l-div-chbox"
                                        checked="{
                                            parts:['oDtaVrfyContracts>/AdvMeter'],
                                            formatter:'._formatChecked'}"/>
                                    <utmain:Label labelFor="chk_advMtr">
                                        <ute:Tag elem="span" text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_AdvMtrChk}"/>
                                    </utmain:Label>

                                    <utmain:CheckBox id="chk_rmoMtr"
                                        class="nrgDashboard-cusDataVerify-l-div-chbox"
                                        checked="{
                                            parts:['oDtaVrfyContracts>/RemoteMeter'],
                                            formatter:'._formatChecked'}"/>
                                    <utmain:Label labelFor="rmo_advMtr">
                                        <ute:Tag elem="span" text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_RmoMtrChk}"/>
                                    </utmain:Label>

                                    <utmain:CheckBox id="chk_proMtr"
                                        class="nrgDashboard-cusDataVerify-l-div-chbox"
                                        checked="{
                                            parts:['oDtaVrfyContracts>/ProvisionMeter'],
                                            formatter:'._formatChecked'}"/>
                                    <utmain:Label labelFor="pro_advMtr">
                                        <ute:Tag elem="span" text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_ProvMtrChk}"/>
                                    </utmain:Label>

                                </ute:Tag>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label labelFor="contacc"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_ContAcc}"
                                                class="nrgDashboard-cusDataVerify-l-div-halflabl"/>
                                            <ute:TextView id="contacc"
                                                text="{oDtaVrfyContracts>/ContractAccountID}"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label labelFor="rtcls_lbl"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_RtCls}"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="rtcls_lbl"
                                                text="{oDtaVrfyContracts>/RateClass}"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <utmain:Label  labelFor="esid_lbl"
                                    text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_ESID}"
                                    class="nrgDashboard-cusDataVerify-l-div-esidLabl"/>
                                <ute:TextView id="esid_lbl"
                                              class="nrgDashboard-cusDataVerify-l-div-tdspVal"
                                              text="{oDtaVrfyContracts>/ESID}"/>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label labelFor="dl_lbl"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_DlNum}"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="dl_lbl"
                                                text="{oDtaVrfyBP>/DL}"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label labelFor="ssn_lbl"
                                                 text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_SSNum}"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="ssn_lbl"
                                                text="{oDtaVrfyBP>/SSN}"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                            </ute:Tag>
                        </l:MatrixLayoutCell>
                        <!--Area Left Ends-->
                        <!--Area Middle (service address/mailing address)-->
                        <l:MatrixLayoutCell class="nrgDashboard-cusDataVerify-m">
                            <ute:Tag class="nrgDashboard-cusDataVerify-mDiv">
                            <ute:ToggleButton design="ToggleDashboard"
                                leftBtnText="{comp-i18n-dashboard>nrgDashboardCusDatVrf_TglBtnLTxt}"
                                rightBtnText="{comp-i18n-dashboard>nrgDashboardCusDatVrf_TglBtnRTxt}"
                                class="nrgDashboard-cusDataVerify-mTgBtn"
                                press="._onToggleButtonPress">
                            </ute:ToggleButton>
                            <ute:Tag class="nrgDashboard-cusDataVerify-mRdBtn">
                                <utmain:RadioButton group="service_mail_radio"/>
                                <m:ScrollContainer height="100%" width="100%" vertical="true">
                                <utmain:Dropdown
                                    content="{oAllContractsofBuag>/}" >
                                    <utmain:DropdownItem
                                        key="{oAllContractsofBuag>iIndex}">
                                        <ute:Tag elem="span"
                                            text="{oAllContractsofBuag>HouseNum1} {oAllContractsofBuag>Street}, {oAllContractsofBuag>City1}"/>
                                    </utmain:DropdownItem>
                                </utmain:Dropdown>
                                </m:ScrollContainer>
                                <c:DropdownBox
                                    items="{oAllContractsofBuag>/}" change="._onContractChange">
                                    <c:items>
                                        <core:ListItem
                                            key="{oAllContractsofBuag>iIndex}"
                                            text="{oAllContractsofBuag>HouseNum1} {oAllContractsofBuag>Street}, {oAllContractsofBuag>City1}"/>
                                    </c:items>
                                </c:DropdownBox>
                                <utmain:RadioButton group="service_mail_radio"
                                    class="nrgDashboard-cusDataVerify-mRdBtn-rdBtn"/>
                                <c:DropdownBox
                                    items="{oAllBuags>/}" change="._onBuagChange">
                                    <c:items>
                                        <core:ListItem
                                            key="{oAllBuags>ContractAccountID}"
                                            text="{oAllBuags>ContractAccountID}"/>
                                    </c:items>
                                </c:DropdownBox>
                            </ute:Tag>
                            <ute:Tag id="serviceadd_area" visible="true" class="nrgDashboard-cusDataVerify-mContent">
                                <ute:TextView class="nrgDashboard-cusDataVerify-mContent-add"
                                    text="{oDtaVrfyContracts>/HouseNum1} {oDtaVrfyContracts>/Street},"/>
                                <ute:TextView class="nrgDashboard-cusDataVerify-mContent-add"
                                    text="{oDtaVrfyContracts>/City1}, {oDtaVrfyContracts>/Region} {oDtaVrfyContracts>/PostCode1}"/>

                                <utmain:Label  labelFor="contractNum"
                                    text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_CaNum}"
                                    class="nrgDashboard-cusDataVerify-mContent-fLbl"/>
                                <ute:Textfield id="contractNum" fieldType="Noborder"
                                    editable="false"
                                    value="{oDtaVrfyContracts>/ContractID}"
                                    class="nrgDashboard-cusDataVerify-mContent-fInput"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label  labelFor="cell_num"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_CellNum}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="cell_num" fieldType="Noborder"
                                                editable="{oCfrmStatus>/bEditable}"
                                                value="{oDtaVrfyBP>/Cell}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="fax_num"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_FaxNum}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="fax_num" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/Fax}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label  labelFor="dayPhn"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_DayPhnTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="dayPhn" fieldType="Noborder"
                                                editable="{oCfrmStatus>/bEditable}"
                                                value="{oDtaVrfyBP>/DayPhone}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="dayPhnType"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_DayPhnTypTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="dayPhnType" fieldType="Noborder"
                                                editable="{oCfrmStatus>/bEditable}"
                                                value="{oDtaVrfyBP>/DayPhoneType}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="evnPhn"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_EvnPhnTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="evnPhn" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/EveningPhone}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="evnPhnType"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_EvnPhnTypTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="evnPhnType" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/EveningPhoneType}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Button text="SMS"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="email_lbl"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_Email}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="email_lbl" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/Email}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>

                            </ute:Tag>
                            <ute:Tag id="mailadd_area" visible="false" class="nrgDashboard-cusDataVerify-mContent">


                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label  labelFor="mail_FixedAddr"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_FAddr}"
                                                class="nrgDashboard-cusDataVerify-mContent-fLbl"/>
                                            <ute:Textfield id="mail_FixedAddr" fieldType="Noborder"
                                                value="{oDtaVrfyMailingTempAddr>/FixAddrInfo/ShortForm}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-fInput"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label  labelFor="mail_TempAddr"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_TAddr}"
                                                class="nrgDashboard-cusDataVerify-mContent-fLbl"/>
                                            <ute:Textfield id="mail_TempAddr" fieldType="Noborder"
                                                value="{oDtaVrfyMailingTempAddr>/TempAddrInfo/ShortForm}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-fInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label  labelFor="cell_num_mail"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_CellNum}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="cell_num_mail" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/Cell}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="fax_num_mail"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_FaxNum}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="fax_num_mail" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/Fax}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label  labelFor="dayPhn_mail"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_DayPhnTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="dayPhn_mail" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/DayPhone}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="dayPhnType_mail"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_DayPhnTypTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="dayPhnType_mail" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/DayPhoneType}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="evnPhn_mail"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_EvnPhnTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="evnPhn_mail" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/EveningPhone}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="evnPhnType_mail"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_EvnPhnTypTxt}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="evnPhnType_mail" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/EveningPhoneType}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Button text="SMS"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label labelFor="email_lbl_mail"
                                                text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_Email}"
                                                class="nrgDashboard-cusDataVerify-mContent-hLbl"/>
                                            <ute:Textfield id="email_lbl_mail" fieldType="Noborder"
                                                value="{oDtaVrfyBP>/Email}"
                                                editable="{oCfrmStatus>/bEditable}"
                                                class="nrgDashboard-cusDataVerify-mContent-hInput"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                            </ute:Tag>
                            </ute:Tag>

                        </l:MatrixLayoutCell>
                        <!--Area Middle Ends-->

                        <!--Area Right (Only 2 buttons )-->
                        <l:MatrixLayoutCell class="nrgDashboard-cusDataVerify-r">
                            <ute:Tag class="nrgDashboard-cusDataVerify-rContent">
                                <utmain:Button id="id_confmBtn" visible="true"
                                    class="nrgDashboard-cusDataVerify-rContent-vBtn"
                                    press="._handleConfirm"
                                    text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_BtnConfitm}"/>
                                <utmain:Button id="id_unConfmBtn" visible="false"
                                    class="nrgDashboard-cusDataVerify-rContent-vBtn"
                                    press="._handleUnConfirm"
                                    text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_BtnUnConfirm}"/>
                                <utmain:Button id="id_updtBtn" enabled="true"
                                    class="nrgDashboard-cusDataVerify-rContent-uBtn"
                                    design="Invert"
                                    press="._handleUpdate"
                                    text="{comp-i18n-dashboard>nrgDashboardCusDatVrf_BtnUpdate}"/>
                            </ute:Tag>
                        </l:MatrixLayoutCell>
                    </l:cells>
                </l:MatrixLayoutRow>
            </l:rows>
        </l:MatrixLayout>
    </ute:Tag>
</mvc:View>

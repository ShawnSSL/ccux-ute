 <!--Routing Pattern: ServiceOrder/bp/{bpNum}/ca/{caNum}/co/{coNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.dashboard.view.ServiceOrder">

	<ute:Tag class="nrgAppMain-generalContent nrgDashboard-cusSerOdr">
        <ute:Tag class="nrgDashboard-cusSerOdr-title" text="{comp-i18n-dashboard>nrgDashboard-ServOrder-ScrnTitle}"/>
        <ute:Tag class="nrgDashboard-cusSerOdr-ctnt">
            <!--Tab Area-->
            <ute:Tag class="nrgDashboard-cusSerOdr-lftArea">
                <ute:Tag visible="{oSelectedTabs>/completeSelected}" class="nrgDashboard-cusSerOdr-lftArea-tab">
                    <utmain:Button class="" design="None"
                        press="._onPendingTabClicked"
                        text="{comp-i18n-dashboard>nrgDashboard-servOrder-Orderlist-PendingOrder}"/>
                </ute:Tag>
                <ute:Tag visible="{oSelectedTabs>/pendingSelected}" class="nrgDashboard-cusSerOdr-lftArea-tab nrgDashboard-cusSerOdr-lftArea-tab-selected">
                    <utmain:Button class="" design="None"
                        text="{comp-i18n-dashboard>nrgDashboard-servOrder-Orderlist-PendingOrder}"/>
                </ute:Tag>
                <ute:Tag visible="{oSelectedTabs>/pendingSelected}" class="nrgDashboard-cusSerOdr-lftArea-tab">
                    <utmain:Button class="" design="None"
                        press="._onCompleteTabClicked"
                        text="{comp-i18n-dashboard>nrgDashboard-servOrder-Orderlist-CompleteOrder}"/>
                </ute:Tag>
                <ute:Tag visible="{oSelectedTabs>/completeSelected}" class="nrgDashboard-cusSerOdr-lftArea-tab nrgDashboard-cusSerOdr-lftArea-tab-selected">
                    <utmain:Button class="" design="None"
                        text="{comp-i18n-dashboard>nrgDashboard-servOrder-Orderlist-CompleteOrder}"/>
                </ute:Tag>
            </ute:Tag>
            <!--Detail Area-->
            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea">
                <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-dpdwn-title">
                    <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-dpdwn-title-lbl"
                             text="{comp-i18n-dashboard>nrgDashboard-servOrder-Address}"/>
                    <utmain:Dropdown id="idESIDDropdown"
                        class="nrgDashboard-cusSerOdr-rgtArea-dpdwn-title-dropdownMenu"
                        selectedKey="{oESIDDropdown>/results/selectedKey}"
                        content="{oESIDDropdown>/results}"
                        select="._onESIDSelect">
                        <utmain:DropdownItem key="{oESIDDropdown>ESID}">
                            <ute:Tag elem="span"
                                     text="{oESIDDropdown>ESID} / {oESIDDropdown>House} {oESIDDropdown>Street} {oESIDDropdown>Apt}, {oESIDDropdown>City} {oESIDDropdown>State} {oESIDDropdown>ZIP}"/>
                        </utmain:DropdownItem>
                    </utmain:Dropdown>
                </ute:Tag>
                <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt">
                    <!--For Movein/Switch-->
                    <ute:Tag visible="{parts:['oSelectedTabs>/pendingSelected', 'oPndingVisType>/visMovein'], formatter:'._formatVisPnd'}">
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-title" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Moviein}" />
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail">
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr-lft">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-labl" elem="span" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Contact}"/>
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-val"  elem="span" text="{oEnrollHolds>/results/0/Name}"/>
                                </ute:Tag>
                                <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr-rgt">
                                    <ute:Tag>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-labl" elem="span" text="{comp-i18n-dashboard>nrgDashboard-servOrder-pMvinDate}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-val" elem="span"
                                                 text="{parts:['oEnrollHolds>/results/0/OrigMviDate'], formatter:'._formatDate'}"/>
                                    </ute:Tag>
                                    <ute:Tag>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-labl" elem="span" text="{comp-i18n-dashboard>nrgDashboard-servOrder-oMvinDate}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-val" elem="span"
                                                 text="{parts:['oEnrollHolds>/results/0/CurrMviDate'], formatter:'._formatDate'}"/>
                                    </ute:Tag>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-ctnt">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                 text="{comp-i18n-dashboard>Type}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                 text="{comp-i18n-dashboard>Reason}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                 text="{comp-i18n-dashboard>Status}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                 text="{comp-i18n-dashboard>CompleteDate}"/>
                                    </ute:Tag>
                                    <ute:Tag content="{oEnrollHolds>/results}">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-row">
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                     text="{comp-i18n-dashboard>nrgDashboard-servOrder-enrl-hold}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                     text="{oEnrollHolds>Reason}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                     text="{oEnrollHolds>Status}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                     text="{parts:['oEnrollHolds>Time'],
                                                            formatter:'._formatDate'}"/>
                                        </ute:Tag>
                                    </ute:Tag>
                                </ute:Tag>
                                <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                 text="{comp-i18n-dashboard>Type}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                 text="{comp-i18n-dashboard>Reason}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                 text="{comp-i18n-dashboard>Status}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                 text="{comp-i18n-dashboard>CompleteDate}"/>
                                    </ute:Tag>
                                    <ute:Tag content="{oEnrollPndingStats>/results}">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-row">
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                     text="{comp-i18n-dashboard>nrgDashboard-servOrder-enrl-pend}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                     text="{oEnrollPndingStats>DESCRIPTION}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                     text="{oEnrollPndingStats>E_BPRC_LL_STATUS}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                     text="{parts:['oEnrollPndingStats>E_BPRC_LLST_DATE'],
                                                            formatter:'._formatDate'}"/>
                                        </ute:Tag>
                                    </ute:Tag>
                                </ute:Tag>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                    <!--For Reconnect-->
                    <ute:Tag visible="{parts:['oSelectedTabs>/pendingSelected', 'oPndingVisType>/visReconnect'], formatter:'._formatVisPnd'}">
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-title" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Reconnect}" />
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail">
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr-lft">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-labl" elem="span" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Contact}"/>
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-val"  elem="span" text=""/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-ctnt">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                 text="{comp-i18n-dashboard>Type}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                 text="{comp-i18n-dashboard>Reason}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                 text="{comp-i18n-dashboard>Status}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                 text="{comp-i18n-dashboard>CompleteDate}"/>
                                    </ute:Tag>
                                    <ute:Tag content="{oReconOrds>/results}">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-row">
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                     text="{oReconOrds>Type}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                     text="{oReconOrds>QMNUM}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                     text="{oReconOrds>Status}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                     text="{parts:['oReconOrds>Time'],
                                                            formatter:'._formatDate'}"/>
                                        </ute:Tag>
                                    </ute:Tag>
                                </ute:Tag>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                    <!--For Disconnect-->
                    <ute:Tag visible="{parts:['oSelectedTabs>/pendingSelected', 'oPndingVisType>/visDisconnect'], formatter:'._formatVisPnd'}">
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-title" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Disconnect}" />
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail">
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr-lft">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-labl" elem="span" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Contact}"/>
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-val"  elem="span" text=""/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-ctnt">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                 text="{comp-i18n-dashboard>Type}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                 text="{comp-i18n-dashboard>Reason}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                 text="{comp-i18n-dashboard>Status}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                 text="{comp-i18n-dashboard>CompleteDate}"/>
                                    </ute:Tag>
                                    <ute:Tag content="{oDiscOrds>/results}">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-row">
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                     text="{oDiscOrds>Type}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                     text="{oDiscOrds>QMNUM}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                     text="{oDiscOrds>Status}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                     text="{parts:['oDiscOrds>Time'],
                                                            formatter:'._formatDate'}"/>
                                        </ute:Tag>
                                    </ute:Tag>
                                </ute:Tag>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                    <!--For Others-->
                    <ute:Tag visible="{parts:['oSelectedTabs>/pendingSelected', 'oPndingVisType>/visOthers'], formatter:'._formatVisPnd'}">
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-title" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Others}" />
                        <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail">
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-hdr-lft">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-labl" elem="span" text="{comp-i18n-dashboard>nrgDashboard-servOrder-Contact}"/>
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdh-val"  elem="span" text=""/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgDashboard-cusSerOdr-rgtArea-ctnt-detail-ctnt">
                                <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl">
                                    <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                 text="{comp-i18n-dashboard>Type}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                 text="{comp-i18n-dashboard>Reason}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                 text="{comp-i18n-dashboard>Status}"/>
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-hdr-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                 text="{comp-i18n-dashboard>CompleteDate}"/>
                                    </ute:Tag>
                                    <ute:Tag content="{oOtherOrds>/results}">
                                        <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-row">
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c1"
                                                     text="{oOtherOrds>Type}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c2"
                                                     text="{oOtherOrds>QMNUM}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c3"
                                                     text="{oOtherOrds>Status}"/>
                                            <ute:Tag class="nrgDashboard-cusSerOdr-rcdc-tbl-cell nrgDashboard-cusSerOdr-rcdc-tbl-hdr-c4"
                                                     text="{parts:['oOtherOrds>Time'],
                                                            formatter:'._formatDate'}"/>
                                        </ute:Tag>
                                    </ute:Tag>
                                </ute:Tag>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>

        </ute:Tag>
    </ute:Tag>
</mvc:View>

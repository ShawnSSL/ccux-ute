<!--Routing Pattern: customerDataVerification/bp/{bpNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    controllerName="nrg.module.dashboard.view.CallerNoIDEnrollment">

    <ute:Tag class="nrgAppMain-generalContent nrgDashboard-cusDataVerify">
        <l:MatrixLayout
            class="nrgDashboard-cusDataVerify-layout"
            widths="288px,534px,84px" columns="3" width="100%">
            <l:rows>
                <l:MatrixLayoutRow>
                    <l:cells>
                        <!--Area Left (active)-->
                        <l:MatrixLayoutCell class="nrgDashboard-cusDataVerify-l">
                            <ute:Tag class="nrgDashboard-cusDataVerify-l-div">
                                <ute:TextView text="ACTIVE"
                                    class="nrgDashboard-cusDataVerify-l-div-title"/>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <utmain:Label text="TDSP" labelFor="tdsp"
                                    class="nrgDashboard-cusDataVerify-l-div-tdspLabl"/>
                                <ute:TextView id="tdsp" text="CENTERPOINT ENERGY"
                                    class="nrgDashboard-cusDataVerify-l-div-tdspVal"/>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label text="METER. #"
                                                labelFor="meter"
                                                class="nrgDashboard-cusDataVerify-l-div-halflabl"/>
                                            <ute:TextView id="meter"
                                                text="123235457"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label text="DESCRIPTION"
                                                labelFor="desp"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="desp"
                                                text="Non interval"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label text="CONTRACT ACC"
                                                labelFor="contacc"
                                                class="nrgDashboard-cusDataVerify-l-div-halflabl"/>
                                            <ute:TextView id="contacc"
                                                text="0000000"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label text="RATE CLASS"
                                                labelFor="rtcls"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="rtcls"
                                                text="RES"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <utmain:Label text="ESID" labelFor="esid"
                                    class="nrgDashboard-cusDataVerify-l-div-esidLabl"/>
                                <ute:TextView id="esid"
                                              class="nrgDashboard-cusDataVerify-l-div-tdspVal"
                                              text="1234567891234567891234"/>

                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label text="DL #" labelFor="dl"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="dl" text="0000000000"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell
                                            class="nrgDashboard-cusDataVerify-l-div-cell">
                                            <utmain:Label text="SS #" labelFor="ssn"
                                                class="nrgDashboard-cusDataVerify-l-div-ritelabl"/>
                                            <ute:TextView id="ssn" text="*****1234"
                                                class="nrgDashboard-cusDataVerify-l-div-halgVal"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                            </ute:Tag>
                        </l:MatrixLayoutCell>
                        <!--Area Left Ends-->
                        <!--Area Middle (service address/mailing address)-->
                        <l:MatrixLayoutCell class="nrgDashboard-cusDataVerify-m">
                            <ute:ToggleButton design="ToggleDashboard"
                                leftBtnText="SERVICE ADDRESS"
                                rightBtnText="MAILING ADDRESS">

                            </ute:ToggleButton>
                            <ute:RadioButton id="service" groupName="ServiceMailing"
                                             text="temp_Server Add"/>
                            <ute:RadioButton id="mailing" groupName="ServiceMailing"
                                             text="temp_Mailing Add"/>
                            <ute:Tag class="nrgDashboard-cusDataVerify-mContact">
                                <ute:TextView text="123 Main St., APT A123"/>
                                <ute:TextView text="Houston, TX 77777"/>

                                <utmain:Label text="CONTRACT #:" labelFor="contractNum"/>
                                <input id="contractNum"/>

                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="CELL:" labelFor="cell1"/>
                                            <input id="cell1"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="CELL 2:" labelFor="cell2"/>
                                            <input id="cell2"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="DAY:" labelFor="dayPhn"/>
                                            <input id="dayPhn"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="TYPE:" labelFor="dayPhnType"/>
                                            <input id="dayPhnType"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="EVENING:" labelFor="evnPhn"/>
                                            <input id="day_phone"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="TYPE:" labelFor="evnPhnType"/>
                                            <input id="evnPhnType"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>
                                <ute:HorizontalDivider
                                    class="nrgDashboard-cusDataVerify-horDvi"/>
                                <l:MatrixLayout widths="50%,50%" columns="2">
                                    <l:MatrixLayoutRow>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="EMAIL:" labelFor="email1"/>
                                            <input id="email1"/>
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell>
                                            <utmain:Label text="EMAIL 2:" labelFor="email2"/>
                                            <input id="email2"/>
                                        </l:MatrixLayoutCell>
                                    </l:MatrixLayoutRow>
                                </l:MatrixLayout>


                            </ute:Tag>
                        </l:MatrixLayoutCell>
                        <!--Area Middle Ends-->
                        <!--Area Right (Only 2 buttons )-->
                        <l:MatrixLayoutCell class="nrgDashboard-cusDataVerify-r">
                            <ute:TextView text="area3"/>
                        </l:MatrixLayoutCell>
                    </l:cells>
                </l:MatrixLayoutRow>
            </l:rows>
        </l:MatrixLayout>
    </ute:Tag>
</mvc:View>

<!--Routing Pattern: customerDataVerification/bp/{bpNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.billing.view.BillingCheckbook">

    <ute:Tag class="nrgAppMain-generalContent nrgBilling-blChkBk">
        <!-- Left -->
        <ute:Tag class="nrgBilling-blChkBk-lftDiv">
            <ute:Tag class="nrgBilling-blChkBk-lftDiv-pageTitle">
                <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-pageTitleSpan"
                         text="{comp-i18n-billing>nrgBilling-billChkBook-PAGE_TITLE}" />
            </ute:Tag>

            <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea">
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft">
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-ctnt">
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-lbl"
                                 text="Start Date:"/>
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-val"
                                 text="06/09/14"/>
                        </ute:Tag>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-ctnt">
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-lbl"
                                 text="End Date:"/>
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-val"
                                 text="08/09/15"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl">
                    <ute:Tag>
                        <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtNum" text="$380.87"/>
                        <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtNum" text="*"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtDscp1" text="AMOUNT DUE"/>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtDscp2" text="for no service interuption"/>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-onHover">
                        <ute:tooltip>
                            <ute:Tooltip openDelay="100" class="nrgBilling-billChkBook-quickRefArea-amount-tooltip">
                                <ute:Tag content="{tip>/employers}">
                                    <ute:Tag text="{tip>lastName}"/>
                                </ute:Tag>
                            </ute:Tooltip>
                        </ute:tooltip>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght">
                    <utmain:Button class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght-btn1" text="PAYMENT OPTIONS"/>
                    <utmain:Button class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght-btn2" text="HIGH BILL FACTORS"/>
                </ute:Tag>
            </ute:Tag>

            <m:ScrollContainer class="nrgBilling-blChkBk-lftDiv-scrllArea" width="752px" height="686px" horizontal="false" vertical="true">
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr">
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c1"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-BillingDate}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c2"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-LineItemDesc}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-Amount}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c4"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-DueDate}" />
                </ute:Tag>
                <ute:Tag content="{emp>/employers}">
                    <utmain:Infoline reverse="true">
                        <utmain:content>
                            <utmain:headerContent>
                                <utmain:Label text="{emp>firstName}"/>
                            </utmain:headerContent>
                            <ute:Tag text="{emp>firstName} {emp>lastName}"/>
                            <ute:Tag content="{bp>/employees}">
                                <ute:Tag text="{bp>firstName} {bp>lastName}"/>
                            </ute:Tag>
                        </utmain:content>
                    </utmain:Infoline>
                </ute:Tag>
            </m:ScrollContainer>
            <!-- Quick Reference Area -->



            <ute:Tag class="nrgBilling-billChkBook-quickRefArea">
                <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-date">
                    <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-dateWrap">
                        <l:MatrixLayout
                            class="nrgBilling-billChkBook-dateTable"
                            layoutFixed="false" widths="40%,60%" columns="2" width="100%">
                            <l:rows>
                                <l:MatrixLayoutRow>
                                    <l:cells>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm1"
                                                     text="Start Date" />
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm2"
                                                     text="6 months ago" />
                                        </l:MatrixLayoutCell>

                                    </l:cells>
                                </l:MatrixLayoutRow>
                                <l:MatrixLayoutRow>
                                    <l:cells>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm1"
                                                     text="End Date" />
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm2"
                                                     text="today: 08/06/15" />
                                        </l:MatrixLayoutCell>

                                    </l:cells>
                                </l:MatrixLayoutRow>
                            </l:rows>
                        </l:MatrixLayout>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-amount">
                    <ute:Tag elem="span" class="nrgBilling-billChkBook-amountNum" text="$380.87"/>
                    <ute:Tag elem="sapn" class="nrgBilling-billChkBook-amountAsterisk" text="*"/>
                    <ute:Tag elem="span" class="nrgBilling-billChkBook-amountDscrpt1" text="current amount due"/>
                    <ute:Tag elem="span" class="nrgBilling-billChkBook-amountDscrpt2" text="for no service interuption"/>
                    <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-amountHover">
                        <ute:tooltip>
                            <ute:Tooltip openDelay="100" class="nrgBilling-billChkBook-quickRefArea-amount-tooltip">
                                <ute:Table width="400px">
                                    <ute:columns>
                                        <ute:TableColumn>
                                            <ute:TextView color="White" text="Invoice Amount Due"></ute:TextView>
                                        </ute:TableColumn>
                                        <ute:TableColumn>
                                            <ute:TextView color="White" text="239.45"></ute:TextView>
                                        </ute:TableColumn>
                                    </ute:columns>
                                    <ute:rows>
                                        <ute:TableRow>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                        </ute:TableRow>
                                        <ute:TableRow>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                        </ute:TableRow>
                                    </ute:rows>
                                </ute:Table>
                            </ute:Tooltip>
                        </ute:tooltip>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-customer">
                    <ute:Button class="nrgBilling-billChkBook-customer-btn"
                                   text="payment options"/>
                    <ute:Button class="nrgBilling-billChkBook-customer-btn"
                                   text="high bill factors"/>
                </ute:Tag>
            </ute:Tag>
            <!-- Main Content Body -->
            <!--<ute:ScrollContainer class="nrgBilling-billChkBook-mainContent" height="380px" horizontal="false" vertical="true">
                <ute:Tag elem="div" class="test"/>
                <ute:Tag elem="div" class="test2"/>
            </ute:ScrollContainer>-->
        </ute:Tag>

        <!-- Right -->
        <ute:Tag class="nrgBilling-blChkBk-rghtDiv">

        </ute:Tag>
    </ute:Tag>


</mvc:View>

<!--Routing Pattern: customerDataVerification/bp/{bpNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.billing.view.BillingCheckbook">

    <ute:Tag class="nrgAppMain-generalContent nrgBilling-blChkBk">
        <!-- Left -->
        <ute:Tag class="nrgBilling-blChkBk-lftDiv">
            <ute:Tag class="nrgBilling-blChkBk-lftDiv-pageTitle">
                <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-pageTitleSpan"
                         text="{comp-i18n-billing>nrgBilling-billChkBook-PAGE_TITLE}" />
            </ute:Tag>

            <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea">
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft">
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-ctnt">
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-lbl"
                                 text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-StaDate}"/>
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-val"
                                 text="{parts:['oChkbkHdr>/InvEndDate'],
                                        formatter:'._getPreSixMonthDate'}"/>
                        </ute:Tag>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-ctnt">
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-lbl"
                                 text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-EndDate}"/>
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-val"
                                 text="{parts:['oChkbkHdr>/InvStrartDate'],
                                        formatter:'._getCurDate'}"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl">
                    <ute:Tag>
                        <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtNum"
                                 text="{path: 'oChkbkHdr>/InvAmount',
                                        type: 'nrg.base.type.Price',
                                        formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                        <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtNum" text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-Astrik}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtDscp1" text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-AmountDue}"/>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtDscp2" text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-NoSrvvInteruption}"/>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-onHover">
                        <ute:tooltip>
                            <ute:Tooltip openDelay="100" class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip">
                                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-otrFrm">
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-innFrm">
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-InvcAmtDue}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue"
                                                 text="{path: 'oChkbkHdr>/InvAmount',
                                                        type: 'nrg.base.type.Price',
                                                        formatOptions: { currencySymbol: ''}}"/>
                                    </ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-innFrm nrgBilling-blChkBk-lftDiv-qkRefArea-redHghlt">
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-DscnctNoticeAmountDue}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue"
                                                 text="{path: 'oChkbkHdr>/DisNoticeAmtDue',
                                                        type: 'nrg.base.type.Price',
                                                        formatOptions: { currencySymbol: ''}}"/>
                                    </ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-innFrm nrgBilling-blChkBk-lftDiv-qkRefArea-yllwHghlt">
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-OthrFeeCharge}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue"
                                                 text="{path: 'oChkbkHdr>/OtherFeeChrg',
                                                        type: 'nrg.base.type.Price',
                                                        formatOptions: { currencySymbol: ''}}"/>
                                    </ute:Tag>
                                    <ute:Tag>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-PmtPlnBal}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue"
                                                 text="{path: 'oChkbkHdr>/PaymtPlnBal',
                                                        type: 'nrg.base.type.Price',
                                                        formatOptions: { currencySymbol: ''}}"/>
                                    </ute:Tag>
                                </ute:Tag>
                                <ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-Total}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue"
                                             text="{path: 'oChkbkHdr>/InvAmount',
                                                    type: 'nrg.base.type.Price',
                                                    formatOptions: { currencySymbol: ''}}"/>
                                </ute:Tag>
                            </ute:Tooltip>
                        </ute:tooltip>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght">
                    <utmain:Button class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght-btn1" text="PAYMENT OPTIONS"/>
                    <utmain:Button class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght-btn2" text="HIGH BILL FACTORS"/>
                </ute:Tag>
            </ute:Tag>

            <m:ScrollContainer class="nrgBilling-blChkBk-lftDiv-scrllArea" width="768px" height="686px" horizontal="false" vertical="true">
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr">
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h1"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-BillingDate}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h2"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-LineItemDesc}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h3"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-Amount}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h4"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-DueDate}" />
                </ute:Tag>
                <ute:Tag content="{oPaymentHdr>/results}">
                    <utmain:Infoline expanded="{oPaymentHdr>bExpand}" reverse="true"
                                     press="._onPaymentHdrClicked"
                                     class="nrgBilling-blChkBk-lftDiv-scrllArea-lnItm">
                        <utmain:headerContent>
                            <ute:Tag visible="{oPaymentHdr>bRegul}"
                                     class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh1"
                                     text="{parts:['oPaymentHdr>BillDate'],
                                            formatter:'._formatDate'}" /> <!--1. nrgBilling-blChkBk-lftDiv-scrllArea-hdr-notify for yellow bgclr-->
                                                                          <!--2. nrgBilling-blChkBk-lftDiv-scrllArea-hdr-alert for red bgclr-->
                            <ute:Tag visible="{oPaymentHdr>bAlert}"
                                     class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh1 nrgBilling-blChkBk-lftDiv-scrllArea-hdr-alert"
                                     text="{parts:['oPaymentHdr>BillDate'],
                                            formatter:'._formatDate'}" />
                            <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh2"
                                     text="{comp-i18n-billing>nrgBilling-billChkBook-PmtHdr-InvAmt} {oPaymentHdr>InvoiceNum}"/>
                            <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh3"
                                     text="{path: 'oPaymentHdr>InvAmtDue',
                                        type: 'nrg.base.type.Price',
                                        formatOptions: { currencySymbol: ''}}"/>
                            <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh4"
                                     text="{parts:['oPaymentHdr>DueDate'],
                                            formatter:'._formatDate'}"/>
                            <ute:Tag visible="true"
                                     class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5 nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell-Clot-grn">
                                <ute:Tag elem="span" text="{oPaymentHdr>sCallOut}" visible="{parts:['oPaymentHdr>sCallOut'], formatter:'._formatNotBppBoolean'}">
                                    <ute:tooltip>
                                        <ute:Tooltip id="ChkbookHdrClOt"
                                            visible="false" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip">
                                            <ute:Tag content="{oPaymentHdr>oCallOut/CallOuts}"> <!--Content here-->
                                                <ute:Tag>
                                                    <ute:Tag visible="{parts:['oPaymentHdr>CallOut'], formatter:'._formatNotBppBoolean'}"
                                                             class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-current"
                                                             text="{oPaymentHdr>CallOut}"/>
                                                    <ute:Tag visible="{parts:['oPaymentHdr>CallOut'], formatter:'._formatBppBoolean'}"
                                                             class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-current nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-underline"
                                                             text="{oPaymentHdr>CallOut}">
                                                        <ute:tooltip>
                                                            <ute:Tooltip visible="{parts:['oPaymentHdr>CallOut'], formatter:'._formatBppBoolean'}"
                                                                class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip">
                                                                <ute:Tag>
                                                                    <ute:Tag text="{oPaymentHdr>BBPAmt}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                                    <ute:Tag text="{oPaymentHdr>BBPAmtAddr}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                                </ute:Tag>
                                                                <ute:Tag>
                                                                    <ute:Tag text="{oPaymentHdr>BBPBal}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                                    <ute:Tag text="{oPaymentHdr>BBPBalAddr}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                                </ute:Tag>
                                                                <ute:Tag>
                                                                    <ute:Tag text="{oPaymentHdr>BBPDefBal}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                                    <ute:Tag text="{oPaymentHdr>BBPDefBalTxt}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                                </ute:Tag>
                                                            </ute:Tooltip>
                                                        </ute:tooltip>
                                                    </ute:Tag>
                                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-canceled"
                                                             text="{oPaymentHdr>CallOut}" visible="false"/>
                                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-inactive"
                                                             text="{oPaymentHdr>CallOut}" visible="false"/>
                                                </ute:Tag>
                                            </ute:Tag>
                                        </ute:Tooltip>
                                    </ute:tooltip>
                                </ute:Tag>
                                <ute:Tag elem="span" text="{oPaymentHdr>sCallOut}" visible="{parts:['oPaymentHdr>sCallOut'], formatter:'._formatBppBoolean'}"
                                         class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-uderline">
                                    <ute:tooltip>
                                        <ute:Tooltip visible="{parts:['oPaymentHdr>sCallOut'], formatter:'._formatBppBoolean'}"
                                            class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip">
                                                <ute:Tag>
                                                    <ute:Tag text="{oPaymentHdr>BBPAmt}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                    <ute:Tag text="{oPaymentHdr>BBPAmtAddr}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                </ute:Tag>
                                                    <ute:Tag>
                                                    <ute:Tag text="{oPaymentHdr>BBPBal}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                    <ute:Tag text="{oPaymentHdr>BBPBalAddr}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                </ute:Tag>
                                                <ute:Tag>
                                                    <ute:Tag text="{oPaymentHdr>BBPDefBal}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                    <ute:Tag text="{oPaymentHdr>BBPDefBalTxt}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                </ute:Tag>
                                        </ute:Tooltip>
                                    </ute:tooltip>
                                </ute:Tag>
                                <ute:Tag elem="span" text="{comp-i18n-billing>nrgBilling-billChkBook-PmtHdr-CallOutPlus}"
                                         visible="{parts:['oPaymentHdr>sCallOut2'], formatter:'._formatTwoClotBoolean'}"/>
                                <ute:Tag elem="span" visible="{parts:['oPaymentHdr>sCallOut2'], formatter:'._formatTwoClotBoolean'}">
                                    <ute:Tag elem="span" text="{oPaymentHdr>sCallOut2}" visible="{parts:['oPaymentHdr>sCallOut2'], formatter:'._formatNotBppBoolean'}"/>
                                    <ute:Tag elem="span" text="{oPaymentHdr>sCallOut2}" visible="{parts:['oPaymentHdr>sCallOut2'], formatter:'._formatBppBoolean'}"
                                             class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-uderline">
                                        <ute:tooltip>
                                        <ute:Tooltip visible="{parts:['oPaymentHdr>sCallOut2'], formatter:'._formatBppBoolean'}"
                                            class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip">
                                                <ute:Tag>
                                                    <ute:Tag text="{oPaymentHdr>BBPAmt}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                    <ute:Tag text="{oPaymentHdr>BBPAmtAddr}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                </ute:Tag>
                                                    <ute:Tag>
                                                    <ute:Tag text="{oPaymentHdr>BBPBal}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                    <ute:Tag text="{oPaymentHdr>BBPBalAddr}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                </ute:Tag>
                                                <ute:Tag>
                                                    <ute:Tag text="{oPaymentHdr>BBPDefBal}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-left"/>
                                                    <ute:Tag text="{oPaymentHdr>BBPDefBalTxt}" class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-BBPtoolltip-rite"/>
                                                </ute:Tag>
                                        </ute:Tooltip>
                                    </ute:tooltip>
                                    </ute:Tag>
                                </ute:Tag>
                            </ute:Tag>
                        </utmain:headerContent>
                        <utmain:content>
                            <ute:Tag> <!--Previous due amount-->
                                <ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c1"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c2"
                                             text="{comp-i18n-billing>nrgBilling-billChkBook-PrvAmtDue}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_1"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_2"
                                             text="{path: 'oPaymentHdr>PaymentSumry/PrvAmtDue',
                                                    type: 'nrg.base.type.Price',
                                                    formatOptions: { currencySymbol: ''}}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c4"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c5"/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag content="{oPaymentHdr>Payments/results}"> <!--Payments-->
                                <ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c1"
                                             text="{oPaymentHdr>PaidDate}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c2"
                                             text="{oPaymentHdr>ItemDesc}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_1"
                                             text="{path: 'oPaymentHdr>Paidamt',
                                                    type: 'nrg.base.type.Price',
                                                    formatOptions: { currencySymbol: ''}}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_2"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c4"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c5"/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag > <!--Balance Forward-->
                                <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-dblUperln">
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c1"
                                             text="{oPaymentHdr>PaidDate}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c2"
                                             text="{comp-i18n-billing>nrgBilling-billChkBook-BalForward}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_1"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_2"
                                             text="{path: 'oPaymentHdr>PaymentSumry/BalFwd',
                                                    type: 'nrg.base.type.Price',
                                                    formatOptions: { currencySymbol: ''}}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c4"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c5"/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag content="{oPaymentHdr>PaymentItems/results}"> <!--PaymentItems-->
                                <!--For single or double underline classes add here ==> class="nrgBilling-blChkBk-lftDiv-scrllArea-snglUndrln nrgBilling-blChkBk-lftDiv-scrllArea-dblUndrln"-->
                                <ute:Tag>
                                <ute:Tag visible="{ parts:['oPaymentHdr>CurrChrgInd'],
                                                    formatter:'._formatBoolCurChrg'}"
                                         class="nrgBilling-blChkBk-lftDiv-scrllArea-snglUperln nrgBilling-blChkBk-lftDiv-scrllArea-snglUndrln">
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c1"
                                             text="{ parts:['oPaymentHdr>Date'],
                                                     formatter:'._formatDate'}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c2"
                                             text="{oPaymentHdr>ItemDesc}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_1"
                                             text="{path: 'oPaymentHdr>ValueLt',
                                                    type: 'nrg.base.type.Price',
                                                    formatOptions: { currencySymbol: ''},
                                                    formatter:'._formatLfRtZroVal'}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_2"
                                             text="{path: 'oPaymentHdr>ValueRt',
                                                    type: 'nrg.base.type.Price',
                                                    formatOptions: { currencySymbol: ''},
                                                    formatter:'._formatLfRtZroVal'}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c4"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c5"/>
                                </ute:Tag>
                                <ute:Tag visible="{ parts:['oPaymentHdr>CurrChrgInd'],
                                                    formatter:'._formatBoolCurChrg_Rev'}">
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c1"

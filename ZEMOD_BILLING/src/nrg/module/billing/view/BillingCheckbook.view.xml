<!--Routing Pattern: customerDataVerification/bp/{bpNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.billing.view.BillingCheckbook">

    <ute:Tag class="nrgAppMain-generalContent nrgBilling-blChkBk">
        <!-- Left -->
        <ute:Tag class="nrgBilling-blChkBk-lftDiv">
            <ute:Tag class="nrgBilling-blChkBk-lftDiv-pageTitle">
                <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-pageTitleSpan"
                         text="{comp-i18n-billing>nrgBilling-billChkBook-PAGE_TITLE}" />
            </ute:Tag>

            <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea">
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft">
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-ctnt">
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-lbl"
                                 text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-StaDate}"/>
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-val"
                                 text="{oChkbkHdr>/InvStrartDate}"/>
                        </ute:Tag>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-ctnt">
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-lbl"
                                 text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-EndDate}"/>
                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-lft-val"
                                 text="{oChkbkHdr>/InvEndDate}"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl">
                    <ute:Tag>
                        <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtNum" text="{oChkbkHdr>/InvAmount}"/>
                        <ute:Tag elem="span" class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtNum" text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-Astrik}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtDscp1" text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-AmountDue}"/>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-amtDscp2" text="{comp-i18n-billing>nrgBilling-billChkBook-Hdr-NoSrvvInteruption}"/>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-mdl-onHover">
                        <ute:tooltip>
                            <ute:Tooltip openDelay="100" class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip">
                                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-otrFrm">
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-innFrm">
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-InvcAmtDue}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue" text="{oChkbkHdr>/InvAmount}"/>
                                    </ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-innFrm nrgBilling-blChkBk-lftDiv-qkRefArea-redHghlt">
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-DscnctNoticeAmountDue}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue" text="{oChkbkHdr>/DisNoticeAmtDue}"/>
                                    </ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-innFrm nrgBilling-blChkBk-lftDiv-qkRefArea-yllwHghlt">
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-OthrFeeCharge}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue" text="{oChkbkHdr>/OtherFeeChrg}"/>
                                    </ute:Tag>
                                    <ute:Tag>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-PmtPlnBal}"/>
                                        <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue" text="{oChkbkHdr>/PaymtPlnBal}"/>
                                    </ute:Tag>
                                </ute:Tag>
                                <ute:Tag>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-HdrHvr-Total}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-tooltip-lnValue" text="{oChkbkHdr>/InvAmount}"/>
                                </ute:Tag>
                            </ute:Tooltip>
                        </ute:tooltip>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght">
                    <utmain:Button class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght-btn1" text="PAYMENT OPTIONS"/>
                    <utmain:Button class="nrgBilling-blChkBk-lftDiv-qkRefArea-rght-btn2" text="HIGH BILL FACTORS"/>
                </ute:Tag>
            </ute:Tag>

            <m:ScrollContainer class="nrgBilling-blChkBk-lftDiv-scrllArea" width="768px" height="686px" horizontal="false" vertical="true">
                <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr">
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h1"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-BillingDate}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h2"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-LineItemDesc}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h3"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-Amount}" />
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-h4"
                             text="{comp-i18n-billing>nrgBilling-billChkBook-Table-DueDate}" />
                </ute:Tag>
                <ute:Tag content="{emp>/employers}">
                    <utmain:Infoline reverse="true" class="nrgBilling-blChkBk-lftDiv-scrllArea-lnItm">
                        <utmain:headerContent>
                            <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh1"
                                     text="01/31/14"/> <!--1. nrgBilling-blChkBk-lftDiv-scrllArea-hdr-notify for yellow bgclr-->
                                                       <!--2. nrgBilling-blChkBk-lftDiv-scrllArea-hdr-alert for red bgclr-->
                            <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh2"
                                     text="Invoicee Amount Due Description"/>
                            <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh3"
                                     text="91.08"/>
                            <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh4"
                                     text="08/31/16"/>
                            <ute:Tag visible="false"
                                     class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5"
                                     text="DPP"/>
                            <ute:Tag visible="true"
                                     class="nrgBilling-blChkBk-lftDiv-scrllArea-hh-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5"
                                     text="+++">
                                <ute:tooltip>
                                    <ute:Tooltip class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip">
                                        <ute:Tag content="{emp>/employers}">
                                            <ute:Tag>
                                                <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-current"
                                                         text="{emp>lastName}"/>
                                                <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-canceled"
                                                         text="{emp>lastName}"/>
                                                <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-hdr-hh5-toolltip-inactive"
                                                         text="{emp>lastName}"/>
                                            </ute:Tag>
                                        </ute:Tag>
                                    </ute:Tooltip>
                                </ute:tooltip>
                            </ute:Tag>
                        </utmain:headerContent>
                        <utmain:content>
                            <ute:Tag content="{bp>/employees}">
                                <ute:Tag> <!--For single or double underline classes add here ==> class="nrgBilling-blChkBk-lftDiv-scrllArea-snglUndrln nrgBilling-blChkBk-lftDiv-scrllArea-dblUndrln"-->
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c1"
                                             text="01/31/14"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c2"
                                             text="{bp>firstName} {bp>lastName}"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_1"
                                             text="223.67"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c3_2"
                                             text="123.23"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c4"
                                             text="08/31/16"/>
                                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-ctnt-cell nrgBilling-blChkBk-lftDiv-scrllArea-hdr-c5"
                                             text=""/>
                                </ute:Tag>
                            </ute:Tag>
                        </utmain:content>
                    </utmain:Infoline>
                </ute:Tag>
                <ute:Tag>
                    <ute:Tag class="nrgBilling-blChkBk-lftDiv-scrllArea-poInvc"
                             text="Post Invoice Date Activities" />
                </ute:Tag>
            </m:ScrollContainer>
        </ute:Tag>

        <!-- Right -->
        <ute:Tag class="nrgBilling-blChkBk-rghtDiv">

        </ute:Tag>
    </ute:Tag>


</mvc:View>

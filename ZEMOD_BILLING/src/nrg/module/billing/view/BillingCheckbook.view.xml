<!--Routing Pattern: customerDataVerification/bp/{bpNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.billing.view.BillingCheckbook">

    <ute:Tag class="nrgAppMain-generalContent nrgBilling-billChkBook">
        <!-- Left -->
        <ute:Tag class="nrgBilling-billChkBook-left">
            <ute:Tag elem="span" class="nrgBilling-billChkBook-pageTitle" text="{comp-i18n-billing>nrgBilling-billChkBook-PAGE_TITLE}" />
            <ute:Tag elem="div" class="clearfix">
                <ute:HorizontalDivider width="16%" class="nrgBilling-billChkBook-horiLine"/>
            </ute:Tag>
            <!-- Quick Reference Area -->
            <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea">
                <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-date">
                    <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-dateWrap">
                        <l:MatrixLayout
                            class="nrgBilling-billChkBook-dateTable"
                            layoutFixed="false" widths="40%,60%" columns="2" width="100%">
                            <l:rows>
                                <l:MatrixLayoutRow>
                                    <l:cells>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm1"
                                                     text="Start Date" />
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm2"
                                                     text="6 months ago" />
                                        </l:MatrixLayoutCell>

                                    </l:cells>
                                </l:MatrixLayoutRow>
                                <l:MatrixLayoutRow>
                                    <l:cells>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm1"
                                                     text="End Date" />
                                        </l:MatrixLayoutCell>
                                        <l:MatrixLayoutCell class="nrgBilling-billChkBook-tableCell">
                                            <ute:Tag elem="div" class="nrgBilling-billChkBook-dateTable-clm2"
                                                     text="today: 08/06/15" />
                                        </l:MatrixLayoutCell>

                                    </l:cells>
                                </l:MatrixLayoutRow>
                            </l:rows>
                        </l:MatrixLayout>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-amount">
                    <ute:Tag elem="span" class="nrgBilling-billChkBook-amountNum" text="$380.87"/>
                    <ute:Tag elem="sapn" class="nrgBilling-billChkBook-amountAsterisk" text="*"/>
                    <ute:Tag elem="span" class="nrgBilling-billChkBook-amountDscrpt1" text="current amount due"/>
                    <ute:Tag elem="span" class="nrgBilling-billChkBook-amountDscrpt2" text="for no service interuption"/>
                    <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-amountHover">
                        <ute:tooltip>
                            <ute:Tooltip openDelay="100" class="nrgBilling-billChkBook-quickRefArea-amount-tooltip">
                                <ute:Table width="400px">
                                    <ute:columns>
                                        <ute:TableColumn>
                                            <ute:TextView color="White" text="Invoice Amount Due"></ute:TextView>
                                        </ute:TableColumn>
                                        <ute:TableColumn>
                                            <ute:TextView color="White" text="239.45"></ute:TextView>
                                        </ute:TableColumn>
                                    </ute:columns>
                                    <ute:rows>
                                        <ute:TableRow>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                        </ute:TableRow>
                                        <ute:TableRow>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                            <ute:TextView color="White" text="dsjfijdsfj.45"></ute:TextView>
                                        </ute:TableRow>
                                    </ute:rows>
                                </ute:Table>
                            </ute:Tooltip>
                        </ute:tooltip>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag elem="div" class="nrgBilling-billChkBook-quickRefArea-customer">
                    <ute:Button class="nrgBilling-billChkBook-customer-btn"
                                   text="payment options"/>
                    <ute:Button class="nrgBilling-billChkBook-customer-btn"
                                   text="high bill factors"/>
                </ute:Tag>
            </ute:Tag>
            <!-- Main Content Body -->
            <ute:ScrollContainer class="nrgBilling-billChkBook-mainContent" height="380px" horizontal="false" vertical="true">
                <ute:Tag elem="div" class="test"/>
                <ute:Tag elem="div" class="test2"/>
            </ute:ScrollContainer>
        </ute:Tag>

        <!-- Right -->
        <ute:Tag class="nrgBilling-billChkBook-right">

        </ute:Tag>
    </ute:Tag>


</mvc:View>

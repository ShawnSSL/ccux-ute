    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.billing.view.Extension">
    <ute:Tag class="nrgAppMain-generalContent nrgBilling-dPmtPln">
        <!--------------------------------------------------------------------EXT Granted--------------------------------------------------------------------------------------->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea"  visible="{oDppScrnControl>/EXTGrant}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-ExtTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link">
                <utmain:Button text="{comp-i18n-billing>nrgBilling-EXT-LinkMain}" design="None" press="._onMain"/>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-ExtGranted}" />
                    <ute:Tag class="nrgBilling-dltl-reason">
                        <ute:Tag class="nrgBilling-dltl-reason-lbl" text="{comp-i18n-billing>nrgBilling-dpp-ReasonGrant}"/>
                        <utmain:Dropdown class="nrgBilling-dltl-reason-select"
                                         content="{oExtExtReasons>/results}"
                                         selectedKey="{oExtExtReasons>/selectedKey}"
                                         select="._onExtReasonSelect">
                            <utmain:DropdownItem key="{oExtExtReasons>ReasonCode}">
                                <ute:Tag elem="span" text="{oExtExtReasons>Reason}" />
                            </utmain:DropdownItem>
                        </utmain:Dropdown>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-dueAmt">
                    <ute:Tag elem="span" class="nrgBilling-dlcd-lbl" text="{comp-i18n-billing>nrgBilling-dpp-CurAmtDue}"/>
                    <ute:Tag elem="span" class="nrgBilling-dlcd-val"
                             text="{path: 'oExtExtensions>/results/0/CurrAmtDue', type: 'nrg.base.type.Price',
                                    formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-hdr">
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k2" text="{comp-i18n-billing>nrgBilling-dpp-Description}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k3" text="{comp-i18n-billing>nrgBilling-dpp-Amt}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k4" text="{comp-i18n-billing>nrgBilling-dpp-DueDte}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k5" text="{comp-i18n-billing>nrgBilling-dpp-DefeDte}"/>
                </ute:Tag>
                <ute:Tag content="{oExtExtensions>/results}" class="nrgBilling-dPmtPln-lftArea-ctnt-tbl">
                    <ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-odd"
                                 visible="{parts:['oExtExtensions>OpenItems/ItemNumber'], formatter:'._isOdd'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k2" text="{oExtExtensions>OpenItems/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k3" text="{path: 'oExtExtensions>OpenItems/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k4" text="{parts:['oExtExtensions>OpenItems/DueDate'], formatter:'._formatDate'}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k5" text="{parts:['oExtExtensions>OpenItems/DefferalDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-evn"
                                 visible="{parts:['oExtExtensions>OpenItems/ItemNumber'], formatter:'._isEven'}">

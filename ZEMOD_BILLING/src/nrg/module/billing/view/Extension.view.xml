                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k3" text="{path: 'oExtExtensions>OpenItems/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k4" text="{parts:['oExtExtensions>OpenItems/DueDate'], formatter:'._formatDate'}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-k5" text="{parts:['oExtExtensions>OpenItems/DefferalDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-newdate">
                    <ute:Tag elem="span" class="nrgBilling-dlcn-lbl"
                        text="{comp-i18n-billing>nrgBilling-dpp-ExtNewDate}"/>
                    <ute:DatePicker id="nrgBilling-dpp-ExtGrantDate-id" change="._handleExtDateChange"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-payment" visible="{oExtEligible>/ExtOverride}">
                <utmain:Label class="nrgBilling-payment-lbl" text="Down Payment Amount"/>
                <ute:Textfield class="nrgBilling-payment-lbl"
                                       fieldType="Regular" width="8rem" value="" id="nrgBilling-ext-dwnPayGrantvalue-id"/>
                <utmain:Label class="nrgBilling-payment-lbl" text="Down Payment Due Date"/>
                <ute:DatePicker width="8rem" class="nrgBilling-payment-lbl" id="nrgBilling-ext-dwnPayDueCreateDate-id"/>
                <utmain:Button class="nrgBilling-payment-lbl" text="Make a Payment"
                               press="._onDownPayment"/>
            </ute:Tag>
            <utmain:Infoline expanded="true" class="nrgBilling-dPmtPln-lftArea-ctnt-infoline">
                <utmain:headerContent>
                    <ute:Tag text="{comp-i18n-billing>nrgDppTool-ContactLogttl}" />
                 </utmain:headerContent>
                <utmain:content>
                    <ute:TextArea value="{oLocalModel>/GrantCL}" width="45rem" class="nrgBilling-dPmtPln-lftArea-ctnt-infoline-textArea" id="idnrgBilling-ExtAccGrantCL"></u:TextArea>
                 </utmain:content>
            </utmain:Infoline>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                   <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox" visible="false">
                    <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox-icon"><ute:Tag text="{comp-i18n-billing>nrgBilling-dpp-info-i}"/></ute:Tag>
                    <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox-txt" text="Customer still owes: "/>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-btn-left">
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCfirm}"
                                   press="._onExtConfirmClick"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCncl}"
                                   press="._onEXTConfCancelClick"/>
                </ute:Tag>
            </ute:Tag>
        </ute:Tag>


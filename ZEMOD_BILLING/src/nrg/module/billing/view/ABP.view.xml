<?xml version="1.0" encoding="UTF-8"?>

<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    xmlns:chart="nrg.module.billing.view.control"
    controllerName="nrg.module.billing.view.ABP"
    displayBlock="true">

    <ute:Tag class="nrgBilling-avgBillingPopup-content">
        <!-------------------------------------------------------- Table ------------------------------------------------------------>
        <ute:Tag class="nrgBilling-avgBillingPopup-amountDueSec" visible="{oABPScrnControl>/Table}">
            <ute:Tag
                elem="sapn"
                text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-TITLE}"
                class="nrgBilling-avgBillingPopup-amountDue-title"/>

            <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-content">
                <!-- Amount History -->
                <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history">
                    <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-labelSec">
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-monthLabel"
                            text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-history-month-LABEL}"/>
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-usageLabel"
                            text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-history-usage-LABEL}"/>
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-usageChargeLabel"
                            text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-history-usageCharge-LABEL}"/>
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-estLabel"
                            text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-history-est-LABEL}"/>
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-adjLabel"
                            text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-history-adj-LABEL}"/>
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-amtUsedLabel"
                            text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-history-amtUsed-LABEL}"/>
                    </ute:Tag>
                    <m:ScrollContainer class="nrgBilling-avgBillingPopup-amountDue-history-scrollArea" height="200px" horizontal="false" vertical="true">
                        <ute:Tag content="{oAmountHistory>/}">
                            <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-row" visible="true">
                                <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-month"
                                    text="{oAmountHistory>Period}"/>
                                <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-usage"
                                    text="{oAmountHistory>Usage}"/>
                                <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-usageCharge"
                                    text="{path: 'oAmountHistory>ActualBill',
                                                            type: 'nrg.base.type.Price',
                                                            formatOptions: { currencySymbol: '$', currencyAlignment: 'LHS', minFractionDigits: '1', maxFractionDigits: '2' }}"/>
                                <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-est"
                                    text="{oAmountHistory>Est}"/>
                                <ute:Textfield class="nrgBilling-avgBillingPopup-amountDue-history-adj"
                                    value="{oAmountHistory>ActualBill}"
                                    editable="{=${oAmountHistory>ActualBill} ? true : false}"
                                    tooltip="{=${oAmountHistory>ActualBill} ? '$0 month' : '$Adjustments'}"
                                    width="60px"/>
                                <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-amtUsed"
                                    text="{oAmountHistory>AmtUsdAbp}"/>
                            </ute:Tag>
                        </ute:Tag>
                    </m:ScrollContainer>
                    <!-- Total -->
                    <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-row">
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-total"
                            text="TOTAL"/>
                        <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-history-totalAmount"
                            text="{oAmountHistory>/totalAmount}"/>
                    </ute:Tag>
                </ute:Tag>
                <!-- Button Section -->
                <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-btnSec">
                    <!-- Bill Amount -->
                    <ute:Tag elem="sapn" text="{oAmountHistory>/estAmount}"
                        class="nrgBilling-avgBillingPopup-amountDue-btnSec-title"/>
                    <!-- NOTICE -->
                    <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-btnSec-noticeSec">
                        <ute:Tag elem="sapn" text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-notice-TITLE}"
                            class="nrgBilling-avgBillingPopup-amountDue-btnSec-noticeTitle"/>
                        <ute:Tag elem="sapn" text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-notice-CONTENT}"
                            class="nrgBilling-avgBillingPopup-amountDue-btnSec-noticeContent"/>
                    </ute:Tag>
                    <!-- Buttons -->
                    <ute:Tag class="nrgBilling-avgBillingPopup-amountDue-btnSec-buttons">
                        <!-- Caculate Btn -->
                        <utmain:Button
                            design="None"
                            visible="{oEligibility>/NonActivated}"
                            class="nrgBilling-avgBillingPopup-amountDue-calBtn"
                            press="_onCalBtnClick">
                            <ute:Tag
                                class="nrgBilling-avgBillingPopup-amountDue-calBtnText"
                                text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-calBtn-TITLE}" />
                        </utmain:Button>
                        <!-- Set-up Btn -->
                        <utmain:Button
                            design="None"
                            visible="{oEligibility>/NonActivated}"
                            class="nrgBilling-avgBillingPopup-amountDue-setBtn"
                            press="_onSetBtnClick">
                            <ute:Tag
                                class="nrgBilling-avgBillingPopup-amountDue-setBtnText"
                                text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-setBtn-TITLE}" />
                        </utmain:Button>
                        <!-- Cancel Btn -->
                        <utmain:Button
                            design="None"
                            visible="{oEligibility>/NonActivated}"
                            class="nrgBilling-avgBillingPopup-amountDue-cancelBtn"
                            press="_onCancelBtnClick">
                            <ute:Tag
                                class="nrgBilling-avgBillingPopup-amountDue-cancelBtnText"
                                text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-cancelBtn-TITLE}" />
                        </utmain:Button>
                        <!-- De-activate Btn -->
                        <utmain:Button
                            design="None"
                            visible="{oEligibility>/Activated}"
                            class="nrgBilling-avgBillingPopup-amountDue-deactBtn"
                            press="_onDeactBtnClick">
                            <ute:Tag
                                class="nrgBilling-avgBillingPopup-amountDue-deactBtnText"
                                text="{comp-i18n-billing>nrgBilling-avgBillingPopup-amountDue-deactBtn-TITLE}" />
                        </utmain:Button>

                    </ute:Tag>

                </ute:Tag>
            </ute:Tag>
        </ute:Tag>

        <!--------------------------------------------------------------------- Note ------------------------------------------------------------>
        <ute:Tag
            elem="sapn"
            text="{comp-i18n-billing>nrgBilling-avgBillingPopup-NOTE}"
            class="nrgBilling-avgBillingPopup-amountDue-note"
            visible="{oABPScrnControl>/Note}"/>

        <!----------------------------------------------------------------------- Graph ---------------------------------------------------------->
        <ute:Tag class="nrgBilling-avgBillingPopup-usageSec" visible="{oABPScrnControl>/Graph}">
            <ute:Tag
                elem="sapn"
                text="{comp-i18n-billing>nrgBilling-avgBillingPopup-usage-TITLE}"
                class="nrgBilling-avgBillingPopup-usage-title"/>
            <ute:Tag class="nrgBilling-avgBillingPopup-usage-content">
                <!-- Graph -->
                <ute:Tag id="nrgBilling-avgBillingPopup-usage-graph" class="nrgBilling-avgBillingPopup-usage-graph">
                </ute:Tag>
                <!-- Graph Legend -->
                <ute:Tag id="nrgBilling-avgBillingPopup-usage-control" class="nrgBilling-avgBillingPopup-usage-control">
                    <ute:Tag visible="false" id="nrgBilling-avgBillingPopup-graphControlBtn-0" class="usageChartLegend">
                        <utmain:Checkbox id="nrgBilling-avgBillingPopup-graphControlChkbox-0" class="usageChartLegend-checkbox" checked="true" select=".onSelected" />
                        <utmain:Label labelFor="2015-intChk" class="usageChartLegend-label">
                            <span>USAGE <ute:Tag elem="span" id="nrgBilling-avgBillingPopup-graphControlText-0"/></span>
                        </utmain:Label>
                    </ute:Tag>
                    <ute:Tag visible="false" id="nrgBilling-avgBillingPopup-graphControlBtn-1" class="usageChartLegend">
                        <utmain:Checkbox id="nrgBilling-avgBillingPopup-graphControlChkbox-1" class="usageChartLegend-checkbox" checked="true" select=".onSelected" />
                        <utmain:Label labelFor="2014-intChk" class="usageChartLegend-label">
                            <span>USAGE <ute:Tag elem="span" id="nrgBilling-avgBillingPopup-graphControlText-1"/></span>
                        </utmain:Label>
                    </ute:Tag>
                    <ute:Tag visible="false" id="nrgBilling-avgBillingPopup-graphControlBtn-2" class="usageChartLegend">
                        <utmain:Checkbox id="nrgBilling-avgBillingPopup-graphControlChkbox-2" class="usageChartLegend-checkbox" checked="true" select=".onSelected" />
                        <utmain:Label labelFor="2013-intChk" class="usageChartLegend-label">
                            <span>USAGE <ute:Tag elem="span" id="nrgBilling-avgBillingPopup-graphControlText-2"/></span>
                        </utmain:Label>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
        </ute:Tag>
        <!-----------------------------------------  Correspondence ------------------------------------------------------------------------->
        <ute:Tag class="nrgBilling-ABP-lftArea"  visible="{oABPScrnControl>/Correspondence}">
            <ute:Tag class="nrgBilling-ABP-lftArea-top">
                <ute:Tag class="nrgBilling-ABP-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-ABP-PageTitle}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-ABP-lftArea-ctnt">
                <ute:Tag class="nrgBilling-ABP-lftArea-ctnt-corr">
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:RadioButton class="nrgBilling-dlcc-chbrow-chb" group="channel" design="Default" enabled="{oDppStepThreeCom>/Fax_Email_Allow}"
                                         checked="{oDppStepThreeCom>/eMailCheck}" />
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-viaEmail}"/>
                        <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                       fieldType="Underlined" width="200px" value="{oDppStepThreeCom>/eMail}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:RadioButton class="nrgBilling-dlcc-chbrow-chb" group="channel" design="Default" enabled="{oDppStepThreeCom>/Fax_Email_Allow}"
                                         checked="{oDppStepThreeCom>/FaxCheck}" />
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-viaFax}"/>
                        <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                       fieldType="Underlined" width="200px" value="{oDppStepThreeCom>/Fax}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow nrgBilling-dlcc-options">
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-to}"/>
                        <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                       fieldType="Underlined" width="15rem" value="{oDppStepThreeCom>/FaxTo}"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-ABP-lftArea-ctnt-corr">
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:RadioButton class="nrgBilling-dlcc-chbrow-chb" group="channel" design="Default"
                                         checked="{oDppStepThreeCom>/AddrCheck}" select="._onComAddrCheck"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-viaMail}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow nrgBilling-dlcc-options">
                        <utmain:RadioButton class="nrgBilling-dlcc-chbrow-chb" group="Address" design="Default"
                                         checked="{olocalAddress>/current}" select="._onCurrentAddCheck"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-avgBillingPopup-CtAddr}"/>

                        <utmain:RadioButton class="nrgBilling-dlcc-chbrow-chb" group="Address" design="Default"
                                         checked="{olocalAddress>/newAdd}" select="._onNewAddCheck"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-avgBillingPopup-NewAddr}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-mail">
                        <ute:Tag class="nrgBilling-dlcc-mail-l">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-incareof}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="12rem" value="{olocalAddress>/Address/co}" editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                        </ute:Tag>

                        <ute:Tag class="nrgBilling-dlcc-mail-l">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-stNum}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                               fieldType="Underlined" width="15rem" value="{olocalAddress>/Address/HouseNo}" editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-stNAme}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="15rem" value="{olocalAddress>/Address/Street}" editable="{olocalAddress>/editable}" />
                            </ute:Tag>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlcc-mail-l">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-aptNo}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                               fieldType="Underlined"
                                               width="15rem"
                                               value="{olocalAddress>/Address/UnitNo}"
                                               editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-poBox}" />
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="15rem" value="{olocalAddress>/Address/PoBox}" editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlcc-mail-l">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-city}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                               fieldType="Underlined" width="15rem" value="{olocalAddress>/Address/City}" editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-state}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                               fieldType="Underlined" width="5rem" value="{olocalAddress>/Address/State}" editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-zip}" />
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="15rem" value="{olocalAddress>/Address/ZipCode}" editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                        </ute:Tag>
                       <ute:Tag class="nrgBilling-dlcc-mail-l">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-country}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="15rem" value="{olocalAddress>/Address/Country}" editable="{olocalAddress>/editable}"/>
                            </ute:Tag>
                       </ute:Tag>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow nrgBilling-dlcc-options" visible="{olocalAddress>/NewAddressflag}">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"  checked="{olocalAddress>/NewAddrCheck}" />
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="Save New Address"/>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-ABP-lftArea-botm">
                <ute:Tag class="nrgBilling-ABP-lftArea-botm-btns">
                    <!--<utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnBack}"/>-->
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnSend}"
                                   press="._postDPPCommunication"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCncl}"
                                   press="._onCancelBtnClick"/>
                </ute:Tag>
            </ute:Tag>

        </ute:Tag>

        <!-----------------------------------------Trillium address check------------------------------------------------------------------------->

        <ute:Tag class="nrgABP-cusDataVerifyEditMail" visible="{oABPScrnControl>/Trillium}">
            <ute:Tag id="idAddrUpdatePopup-HdrLn" visible="{olocalAddress>/updateSent}"
                class="nrgABP-cusDataVerifyEditMail-hdr">
                <ute:Tag>
                    <core:Icon src="sap-icon://nrg-icon/notification"/>
                    <ute:TextView text="{olocalAddress>/SuggAddrInfo/HeaderText1}"/>
                </ute:Tag>
                <ute:Tag>
                    <core:Icon src="sap-icon://nrg-icon/notification"/>
                    <ute:TextView text="{olocalAddress>/SuggAddrInfo/HeaderText2}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag id="idAddrUpdatePopup-l"
                class="nrgABP-cusDataVerifyEditMail-l">
                <ute:TextView visible="{olocalAddress>/updateSent}"
                    class="nrgABP-cusDataVerifyEditMail-rl-title"
                    text="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_lTitle}"/>
                <ute:Tag><ute:Textfield id="idEditPoBox" fieldType="Underlined"
                    change="._onPoBoxEdit"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_PoBox}"
                    editable="{olocalAddress>/updateNotSent}" width="100px"
                    value="{olocalAddress>/Address/PoBox}"/></ute:Tag>
                <ute:Tag><ute:Textfield id="idEditHouseNum" fieldType="Underlined"
                    change="._onRegAddrEdit"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_HuNum}"
                    editable="{olocalAddress>/updateNotSent}" width="100px"
                    value="{olocalAddress>/Address/HouseNo}"/></ute:Tag>
                <ute:Tag><ute:Textfield id="idEditUnitNum" fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_UniNum}"
                    editable="{olocalAddress>/updateNotSent}" width="100px"
                    value="{olocalAddress>/Address/UnitNo}"/></ute:Tag>
                <ute:Tag><ute:Textfield id="idEditStName" fieldType="Underlined"
                    change="._onRegAddrEdit"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_StName}"
                    editable="{olocalAddress>/updateNotSent}" width="220px"
                    value="{olocalAddress>/Address/Street}"/></ute:Tag>
                <ute:Tag><ute:Textfield id="idEditCity" fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_City}"
                    editable="{olocalAddress>/updateNotSent}" width="100px"
                    value="{olocalAddress>/Address/City}"/></ute:Tag>
                <ute:Tag><ute:Textfield id="idEditState" fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_State}"
                    editable="{olocalAddress>/updateNotSent}" width="100px"
                    value="{olocalAddress>/Address/State}"/></ute:Tag>
                <ute:Tag><ute:Textfield id="idEditZip" fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_ZipCode}"
                    editable="{olocalAddress>/updateNotSent}" width="100px"
                    value="{olocalAddress>/Address/ZipCode}"/></ute:Tag>

                <ute:Button  id="idEditMailAddr_UpdtBtn"
                    visible="{olocalAddress>/updateNotSent}"
                    class="nrgABP-cusDataVerifyEditMailAdd-updtBtn nrgABP-cusDataVerify-alignCenBtn"
                    text="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_UpdtBtnText}"
                    press="._TrilliumAddressCheck"/>
            </ute:Tag>
            <ute:Tag id="idAddrUpdatePopup-r"
                class="nrgABP-cusDataVerifyEditMail-r"
                visible="{olocalAddress>/updateSent}">
                <ute:TextView
                    class="nrgABP-cusDataVerifyEditMail-rl-title"
                    text="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_rTitle}"/>
                <ute:Tag><ute:Textfield fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_PoBox}"
                    editable="false" width="100px"
                    value="{olocalAddress>/SuggAddrInfo/PoBox}"/></ute:Tag>
                <ute:Tag><ute:Textfield fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_HuNum}"
                    editable="false" width="100px"
                    value="{olocalAddress>/SuggAddrInfo/HouseNo}"/></ute:Tag>
                <ute:Tag><ute:Textfield fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_UniNum}"
                    editable="false" width="100px"
                    value="{olocalAddress>/SuggAddrInfo/UnitNo}"/></ute:Tag>
                <ute:Tag><ute:Textfield fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_StName}"
                    editable="false" width="220px"
                    value="{olocalAddress>/SuggAddrInfo/Street}"/></ute:Tag>
                <ute:Tag><ute:Textfield fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_City}"
                    editable="false" width="100px"
                    value="{olocalAddress>/SuggAddrInfo/City}"/></ute:Tag>
                <ute:Tag><ute:Textfield fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_State}"
                    editable="false" width="100px"
                    value="{olocalAddress>/SuggAddrInfo/State}"/></ute:Tag>
                <ute:Tag><ute:Textfield id="idValdZip" fieldType="Underlined"
                    label="{comp-i18n-bupa>nrgBupaCusDatVrf_Pup_ZipCode}"
                    editable="false" width="100px"
                    value="{olocalAddress>/SuggAddrInfo/ZipCode}"/></ute:Tag>
                <ute:Tag id="idAddrUpdatePopup-FtrLn"
                    class="nrgABP-cusDataVerifyEditMail-l-ftr">
                    <ute:TextView text="{olocalAddress>/SuggAddrInfo/FooterLine1}"/>
                    <ute:TextView text="{olocalAddress>/SuggAddrInfo/FooterLine2}"/>
                    <ute:TextView text="{olocalAddress>/SuggAddrInfo/FooterLine3}"/>
                </ute:Tag>

                <ute:Tag class="nrgABP-cusDataVerifyEditMail-r-ChkBx">

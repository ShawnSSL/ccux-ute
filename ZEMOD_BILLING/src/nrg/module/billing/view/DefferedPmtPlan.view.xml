<!--Routing Pattern: billing/highbill/bp/{bpNum}/ca/{caNum}/co/{coNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.billing.view.DefferedPmtPlan">
    <ute:Tag class="nrgAppMain-generalContent nrgBilling-dPmtPln">
        <!--STEP 1-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea" visible="{oDppScrnControl>/StepOne}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link" text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-StepOne}"/>
                    <ute:Tag class="nrgBilling-dltl-reason">
                        <ute:Tag class="nrgBilling-dltl-reason-lbl" text="{comp-i18n-billing>nrgBilling-dpp-ReasonDpp}"/>
                        <utmain:Dropdown class="nrgBilling-dltl-reason-select"
                                         selectedKey="{oDppReasons>/selectedKey}"
                                         content="{oDppReasons>/}"
                                         select="._onReasonSelect" >
                            <utmain:DropdownItem key="{oDppReasons>ReasonCode}">
                                <ute:Tag elem="span" text="{oDppReasons>Reason}" />
                            </utmain:DropdownItem>
                        </utmain:Dropdown>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-m">
                    <ute:Tag class="nrgBilling-dltm-price" text="$75.44"/>
                    <ute:Tag class="nrgBilling-dltm-dplbl" text="{comp-i18n-billing>nrgBilling-dpp-DownPmtAmt}"/>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-r">
                    <utmain:Button class="nrgBilling-dltr-btn" text="{comp-i18n-billing>nrgBilling-dpp-PmtOptions}"/>
                    <ute:Tag class="nrgBilling-dltr-dueDate" text="{comp-i18n-billing>nrgBilling-dpp-DueDate} 10/30/2014"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-hdr">
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox checked="{oDppSetUps>/results/bSelectedAll}" select="._onSelectAllCheck"/></ute:Tag>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h2" text="{comp-i18n-billing>nrgBilling-dpp-Description}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h3" text="{comp-i18n-billing>nrgBilling-dpp-Amt}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h4" text="{comp-i18n-billing>nrgBilling-dpp-DueDte}"/>
                </ute:Tag>
                <ute:Tag content="{oDppSetUps>/results}" class="nrgBilling-dPmtPln-lftArea-ctnt-tbl">
                    <ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-odd"
                                 visible="{parts:['oDppSetUps>iIndex'], formatter:'._isOdd'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1">
                                <utmain:Checkbox checked="{oDppSetUps>OpenItems/bSelected}" select="._onOneItemCheck"/></ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h2" text="{oDppSetUps>OpenItems/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h3"
                                     text="{path: 'oDppSetUps>OpenItems/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h4" text="{parts:['oDppSetUps>OpenItems/DueDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-evn"
                                 visible="{parts:['oDppSetUps>iIndex'], formatter:'._isEven'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox checked="{oDppSetUps>OpenItems/bSelected}"/></ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h2" text="{oDppSetUps>OpenItems/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h3"
                                     text="{path: 'oDppSetUps>OpenItems/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h4" text="{parts:['oDppSetUps>OpenItems/DueDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-dte">
                    <ute:Tag class="nrgBilling-dlbd-left">
                        <utmain:RadioButton class="nrgBilling-dlbd-left-elem nrgBilling-dlbd-left-radio" checked="true"/>
                        <ute:Tag class="nrgBilling-dlbd-left-elem" text="{comp-i18n-billing>nrgBilling-dpp-Mondth}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbd-rght">
                        <ute:Tag class="nrgBilling-dlbd-left-elem" text="{comp-i18n-billing>nrgBilling-dpp-StartDate} "/>
                        <ute:Tag class="nrgBilling-dlbd-left-elem-date" text="{parts:['oDppSetUps>/results/0/StartDate'], formatter:'._formatDate'}"/>


                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbd-install">
                        <ute:Tag elem="span" text="{comp-i18n-billing>nrgBilling-dpp-NmbrOfInstall}"/>
                        <ute:Textfield class="nrgBilling-dlbd-install-Num" fieldType="Underlined" width="22px"
                                       editable="{parts:['oDppSetUps>/results/0/InstlmntNoAuth'], formatter:'._postiveXFormatter'}"
                                       value="{oDppStepOnePost>/InstlmntNo}"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-zeroDown">
                    <utmain:CheckBox class="nrgBilling-dlbz-chk"
                                     enabled="{parts:['oDppSetUps>/results/0/ZeroDwPayAuth'],
                                               formatter:'._postiveXFormatter'}"
                                     checked="{oDppStepOnePost>/ZeroDwPay}"/>
                    <ute:Tag class="nrgBilling-dlbz-chkLabel" text="{comp-i18n-billing>nrgBilling-dpp-PlanZroDownPmt}"/>
                </ute:Tag>
                <utmain:Button class="nrgBilling-dPmtPln-lftArea-botm-btn" text="{comp-i18n-billing>nrgBilling-dpp-OK}" press="._onDppSetUpOk"/>
            </ute:Tag>
        </ute:Tag>
        <!--Step 2-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea" visible="{oDppScrnControl>/StepTwo}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link" text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-StepTwo}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-hdr">
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox/></ute:Tag>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i2" text="{comp-i18n-billing>nrgBilling-dpp-Amt}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i3" text="{comp-i18n-billing>nrgBilling-dpp-DueDte}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i4" text="{comp-i18n-billing>nrgBilling-dpp-ClearDate}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i5" text="{comp-i18n-billing>nrgBilling-dpp-Cleared}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i6" text="{comp-i18n-billing>nrgBilling-dpp-ClearAmt}"/>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-tbl">
                    <ute:Tag class="nrgBilling-dlct-odd">
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox/></ute:Tag>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i2" text="$75.44"/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i3" text="10/30/2014"/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i4" text=""/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i5" text="NO"/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i6" text="$00.00"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlct-evn">
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox/></ute:Tag>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i2" text="$15.09"/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i3" text=""/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i4" text=""/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i5" text="NO"/>
                        <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i6" text="$00.00"/>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox">
                    <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox-icon"><ute:Tag text="{comp-i18n-billing>nrgBilling-dpp-info-i}"/></ute:Tag>
                    <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox-txt" text="The first installment is down payment. The first installment is due on 10/30/2014
Additional installments will be due with the following 5 subsequent invoices."/>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoSum">
                    <ute:Tag class="nrgBilling-dlbi-l">
                        <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                            <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-TtlOutstanding}"/>
                            <ute:Tag class="nrgBilling-dlbi-val" text="$150.87"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                            <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-DppTtl}"/>
                            <ute:Tag class="nrgBilling-dlbi-val" text="$150.87"/>
                        </ute:Tag>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbi-m">
                        <ute:Tag class="nrgBilling-dlbi-l">
                            <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                                <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-SelTtl}"/>
                                <ute:Tag class="nrgBilling-dlbi-val" text="$150.87"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                                <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-Diff}"/>
                                <ute:Tag class="nrgBilling-dlbi-val" text="$00.00"/>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbi-r">
                        <utmain:Button text="{comp-i18n-billing>nrgBilling-dpp-DisDiff}"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-btns">
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnBack}"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCfirm}"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCncl}"/>
                </ute:Tag>
            </ute:Tag>
        </ute:Tag>
        <!--Step 3-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea"  visible="{oDppScrnControl>/StepThree}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link" text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-StepTre}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-corr">
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="Via Email:"/>
                        <utmain:Dropdown class="nrgBilling-dlcc-chbrow-sel">
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option1" />
                            </utmain:DropdownItem>
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option2" />
                            </utmain:DropdownItem>
                        </utmain:Dropdown>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="Via Fax:"/>
                        <utmain:Dropdown class="nrgBilling-dlcc-chbrow-sel">
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option1" />
                            </utmain:DropdownItem>
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option2" />
                            </utmain:DropdownItem>
                        </utmain:Dropdown>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="In care of:"/>
                        <ute:Tag class="nrgBilling-dlcc-chbrow-val" text="test"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-corr">
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="Via Mail:"/>
                        <utmain:Dropdown class="nrgBilling-dlcc-chbrow-sel">
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option1" />
                            </utmain:DropdownItem>
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option2" />
                            </utmain:DropdownItem>
                        </utmain:Dropdown>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-mail">
                        <ute:Tag class="nrgBilling-dlcc-mail-l">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="In care of:"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="330px" text="test"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <ute:Tag class="nrgBilling-dlcc-mail-row-column">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="Street Number:"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="90px" text="test"/>
                                </ute:Tag>
                                <ute:Tag class="nrgBilling-dlcc-mail-row-column">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="Apt./Unit:"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="120px" text="test"/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <ute:Tag class="nrgBilling-dlcc-mail-row-colCity">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="City:"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="232px" text="test"/>
                                </ute:Tag>
                                <ute:Tag class="nrgBilling-dlcc-mail-row-colState">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="State:"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="77px" text="test"/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="Country:"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="340px" text="test"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="Address Title:"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="307px" text="test"/>
                            </ute:Tag>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlcc-mail-r">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="Street Name:"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="177px" text="test"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="PO Box:"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="214px" text="test"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="ZIP:"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="243px" text="test"/>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="Save New Address"/>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-btns">
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnBack}"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnSend}"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCncl}"/>
                </ute:Tag>
            </ute:Tag>
        </ute:Tag>
        <!--DPP Denied-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea"  visible="{oDppScrnControl>/DPPDenied}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link" text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-DppDenied}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-hdr">
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j1" text="{comp-i18n-billing>nrgBilling-dpp-Description}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j2" text="{comp-i18n-billing>nrgBilling-dpp-Amt}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j3" text="{comp-i18n-billing>nrgBilling-dpp-DueDte}"/>
                </ute:Tag>
                <ute:Tag content="{oDppDeniedReason>/results}" class="nrgBilling-dPmtPln-lftArea-ctnt-tbl">
                    <ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-odd" visible="{parts:['oDppDeniedReason>DPPDenyed/iIndex'], formatter:'._isOdd'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j1" text="{oDppDeniedReason>DPPDenyed/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j2"
                                     text="{path: 'oDppDeniedReason>DPPDenyed/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j3" text="{parts:['oDppDeniedReason>DPPDenyed/DueDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-evn" visible="{parts:['oDppDeniedReason>DPPDenyed/iIndex'], formatter:'._isEven'}">

<!--Routing Pattern: billing/highbill/bp/{bpNum}/ca/{caNum}/co/{coNum}-->
<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:c="sap.ui.commons"
    xmlns:l="sap.ui.commons.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    controllerName="nrg.module.billing.view.DefferedPmtPlan">
    <ute:Tag class="nrgAppMain-generalContent nrgBilling-dPmtPln">
        <!--STEP 1-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea" visible="{oDppScrnControl>/StepOne}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link">
                <utmain:Button text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}" design="None" press="._onCheckbook"/>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-StepOne}"/>
                    <ute:Tag class="nrgBilling-dltl-reason" visible="{parts:['oDppEligible>/DPPReasonYes'], formatter:'._reverseBooleanFormatter'}">
                        <ute:Tag class="nrgBilling-dltl-reason-lbl" text="{comp-i18n-billing>nrgBilling-dpp-ReasonDpp}"/>
                        <utmain:Dropdown class="nrgBilling-dltl-reason-select"
                                         selectedKey="{oDppReasons>/selectedKey}"
                                         content="{oDppReasons>/}"
                                         select="._onReasonSelect" >
                            <utmain:DropdownItem key="{oDppReasons>ReasonCode}">
                                <ute:Tag elem="span" text="{oDppReasons>Reason}" />
                            </utmain:DropdownItem>
                        </utmain:Dropdown>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dltl-reason" visible="{oDppEligible>/DPPReasonYes}">
                        <ute:Tag class="nrgBilling-dltl-reason-lbl" text="{comp-i18n-billing>nrgBilling-dpp-ReasonDpp}"/>
                        <ute:Tag class="nrgBilling-dltl-reason-lbl" text="{comp-i18n-billing>nrgBilling-dpp-ReasonDpp}"/>
                    </ute:Tag>
                </ute:Tag>
                <!--<ute:Tag class="nrgBilling-dPmtPln-lftArea-top-m">
                    <ute:Tag class="nrgBilling-dltm-price" text="$75.44"/>
                    <ute:Tag class="nrgBilling-dltm-dplbl" text="{comp-i18n-billing>nrgBilling-dpp-DownPmtAmt}"/>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-r">
                    <utmain:Button class="nrgBilling-dltr-btn" text="{comp-i18n-billing>nrgBilling-dpp-PmtOptions}"/>
                    <ute:Tag class="nrgBilling-dltr-dueDate" text="{comp-i18n-billing>nrgBilling-dpp-DueDate} 10/30/2014"/>
                </ute:Tag>-->
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-hdr">
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox checked="{oDppSetUps>/results/bSelectedAll}" select="._onSelectAllCheck"/></ute:Tag>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h2" text="{comp-i18n-billing>nrgBilling-dpp-Description}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h3" text="{comp-i18n-billing>nrgBilling-dpp-Amt}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h4" text="{comp-i18n-billing>nrgBilling-dpp-DueDte}"/>
                </ute:Tag>
                <ute:Tag content="{oDppSetUps>/results}" class="nrgBilling-dPmtPln-lftArea-ctnt-tbl">
                    <ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-odd"
                                 visible="{parts:['oDppSetUps>OpenItems/ItemNumber'], formatter:'._isOdd'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1">
                                <utmain:Checkbox checked="{oDppSetUps>OpenItems/bSelected}" select="._onOneItemCheck"/></ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h2" text="{oDppSetUps>OpenItems/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h3"
                                     text="{path: 'oDppSetUps>OpenItems/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h4" text="{parts:['oDppSetUps>OpenItems/DueDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-evn"
                                 visible="{parts:['oDppSetUps>OpenItems/ItemNumber'], formatter:'._isEven'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox checked="{oDppSetUps>OpenItems/bSelected}"/></ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h2" text="{oDppSetUps>OpenItems/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h3"
                                     text="{path: 'oDppSetUps>OpenItems/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h4" text="{parts:['oDppSetUps>OpenItems/DueDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-dte">
                    <ute:Tag class="nrgBilling-dlbd-left">
                        <utmain:RadioButton class="nrgBilling-dlbd-left-elem nrgBilling-dlbd-left-radio" checked="true"/>
                        <ute:Tag class="nrgBilling-dlbd-left-elem" text="{comp-i18n-billing>nrgBilling-dpp-Mondth}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbd-rght">
                        <ute:Tag class="nrgBilling-dlbd-left-elem" text="{comp-i18n-billing>nrgBilling-dpp-StartDate} "/>
                        <!--<ute:Tag class="nrgBilling-dlbd-left-elem-date" text="{parts:['oDppSetUps>/results/0/StartDate'], formatter:'._formatDate'}"/>-->
                        <ute:DatePicker id="nrgBilling-dpp-DppStartDate-id"/>

                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbd-install">
                        <ute:Tag elem="span" text="{comp-i18n-billing>nrgBilling-dpp-NmbrOfInstall}"/>
                        <ute:Textfield class="nrgBilling-dlbd-install-Num" fieldType="Underlined" width="22px"
                                       editable="{parts:['oDppSetUps>/results/0/InstlmntNoAuth'], formatter:'._postiveXFormatter'}"
                                       value="{oDppStepOnePost>/InstlmntNo}"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-zeroDown">
                    <utmain:Checkbox class="nrgBilling-dlbz-chk"
                                     enabled="{parts:['oDppSetUps>/results/0/ZeroDwPayAuth'],
                                               formatter:'._postiveXFormatter'}"
                                     checked="{parts:['oDppStepOnePost>/ZeroDwnPay'], formatter:'._postiveXFormatter'}"/>
                    <ute:Tag class="nrgBilling-dlbz-chkLabel" text="{comp-i18n-billing>nrgBilling-dpp-PlanZroDownPmt}"/>
                </ute:Tag>
                <utmain:Button class="nrgBilling-dPmtPln-lftArea-botm-btn" text="{comp-i18n-billing>nrgBilling-dpp-OK}" press="._onDppSetUpOk"/>
            </ute:Tag>
        </ute:Tag>
        <!--Step 2-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea" visible="{oDppScrnControl>/StepTwo}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link">
                <utmain:Button text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}" design="None" press="._onCheckbook"/>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-StepTwo}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-hdr">
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1"><utmain:Checkbox checked="{oDppConfs>/AllChecked}" select="._onDppConfAllCheck"/></ute:Tag>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i2" text="{comp-i18n-billing>nrgBilling-dpp-Amt}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i3" text="{comp-i18n-billing>nrgBilling-dpp-DueDte}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i4" text="{comp-i18n-billing>nrgBilling-dpp-ClearDate}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i5" text="{comp-i18n-billing>nrgBilling-dpp-Cleared}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i6" text="{comp-i18n-billing>nrgBilling-dpp-ClearAmt}"/>
                </ute:Tag>
                <ute:Tag content="{oDppConfs>/results}" class="nrgBilling-dPmtPln-lftArea-ctnt-tbl">
                    <ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-odd"
                                 visible="{parts:['oDppConfs>ConfirmdItems/ItemNumber'], formatter:'._isOdd'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1">
                                <utmain:Checkbox checked="{oDppConfs>Checked}" select="._onDppConfItemCheck"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i2">
                                <ute:Tag elem="span" text="$"/>
                                <ute:Textfield fieldType="Noborder" width="100px" value="{oDppConfs>ConfirmdItems/Amount}"
                                               change="._onStepTwoInstlChange"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i3" text="{parts:['oDppConfs>ConfirmdItems/DueDate'], formatter:'._formatDate'}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i4" text="{parts:['oDppConfs>ConfirmdItems/ClearDate'], formatter:'._formatDate'}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i5" text="{oDppConfs>ConfirmdItems/Cleared}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i6"
                                     text="{path: 'oDppConfs>ConfirmdItems/ClearedAmt', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-evn"
                                 visible="{parts:['oDppConfs>ConfirmdItems/ItemNumber'], formatter:'._isEven'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-h1">
                                <utmain:Checkbox checked="{oDppConfs>Checked}" select="._onDppConfItemCheck"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i2">
                                <ute:Tag elem="span" text="$"/>
                                <ute:Textfield fieldType="Noborder" width="100px" value="{oDppConfs>ConfirmdItems/Amount}"
                                               change="._onStepTwoInstlChange"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i3" text="{parts:['oDppConfs>ConfirmdItems/DueDate'], formatter:'._formatDate'}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i4" text="{parts:['oDppConfs>ConfirmdItems/ClearDate'], formatter:'._formatDate'}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i5" text="{oDppConfs>ConfirmdItems/Cleared}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-i6"
                                     text="{path: 'oDppConfs>ConfirmdItems/ClearedAmt', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                        </ute:Tag>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dlct-date" visible="{parts:['oDppStepOnePost>/ZeroDwnPay'], formatter:'._negativeXFormatter'}">
                        <ute:Tag class="nrgBilling-dlct-date-lbl" text="{comp-i18n-billing>nrgBilling-dpp-FirstInstlDate} "/>
                        <ute:DatePicker id="nrgBilling-dpp-DppDueDate-id"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-payment" visible="false">
                <utmain:Label class="nrgBilling-payment-lbl" text="Down Payment Amount"/>
                <ute:Textfield class="nrgBilling-payment-lbl"
                                       fieldType="Regular" width="8rem" value="0" id="nrgBilling-dpp-dwnPayvalue-id"/>
                <utmain:Label class="nrgBilling-payment-lbl" text="Down Payment Due Date" />
                <ute:DatePicker width="8rem" class="nrgBilling-payment-lbl" id="nrgBilling-dpp-dwnPayDueDate-id" />
                <utmain:Button class="nrgBilling-payment-lbl" text="Make a Payment"
                               press="._onDownPayment"/>
            </ute:Tag>
            <utmain:Infoline expanded="true" class="nrgBilling-dPmtPln-lftArea-ctnt-infoline">
                <utmain:headerContent>
                    <ute:Tag text="{comp-i18n-billing>nrgDppTool-ContactLogttl}" />
                 </utmain:headerContent>
                <utmain:content>
                    <ute:TextArea value="{oExtExtensions>/Message}" width="45rem" class="nrgBilling-dPmtPln-lftArea-ctnt-infoline-textArea" id="idnrgBilling-DPPAccCL"></u:TextArea>
                 </utmain:content>
            </utmain:Infoline>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox">
                    <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox-icon"><ute:Tag text="{comp-i18n-billing>nrgBilling-dpp-info-i}"/></ute:Tag>
                    <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoBox-txt">
                        <ute:Tag visible="{parts:['oDppStepOnePost>/ZeroDwnPay'], formatter:'._negativeXFormatter'}"
                                 elem="span" text="{comp-i18n-billing>nrgBilling-dpp-info-first-zroDwn} "/>
                        <ute:Tag elem="span" text="{comp-i18n-billing>nrgBilling-dpp-info-first} "/>
                        <ute:Tag elem="span" text="{parts:['oDppConfs>/results/0/ConfirmdItems/DueDate'], formatter:'._formatDate'}"/>
                        <ute:Tag elem="span" text="{comp-i18n-billing>nrgBilling-dpp-info-second} "/>
                        <ute:Tag elem="span" text="{oDppStepOnePost>/InstlmntNo} "/>
                        <ute:Tag elem="span" text=" {comp-i18n-billing>nrgBilling-dpp-info-third}"/>

                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-infoSum">
                    <ute:Tag class="nrgBilling-dlbi-l">
                        <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                            <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-TtlOutstanding}"/>
                            <ute:Tag class="nrgBilling-dlbi-val"
                                     text="{path: 'oDppConfs>/results/0/TotOutStd', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                            <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-DppTtl}"/>
                            <ute:Tag class="nrgBilling-dlbi-val"
                                     text="{path: 'oDppConfs>/results/0/DppTot', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                        </ute:Tag>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbi-m">
                        <ute:Tag class="nrgBilling-dlbi-l">
                            <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                                <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-SelTtl}"/>
                                <ute:Tag class="nrgBilling-dlbi-val"
                                         text="{path: 'oDppConfs>/results/0/SelTot', type: 'nrg.base.type.Price',
                                                formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dPmtPln-dlbi-row">
                                <ute:Tag class="nrgBilling-dlbi-lbl" text="{comp-i18n-billing>nrgBilling-dpp-Diff}"/>
                                <ute:Tag class="nrgBilling-dlbi-val"
                                         text="{path: 'oDppConfs>/results/0/DiffTot', type: 'nrg.base.type.Price',
                                                formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlbi-r">
                        <utmain:Button text="{comp-i18n-billing>nrgBilling-dpp-DisDiff}" press="._onDistributeDiffClick"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-btns">
                    <!--<utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnBack}"/>-->
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCfirm}" press="._onDppConfConfirmClick"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCncl}" press="._onDppConfCancelClick"/>
                </ute:Tag>
            </ute:Tag>
        </ute:Tag>
        <!--Step 3-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea"  visible="{oDppScrnControl>/StepThree}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link">
                <utmain:Button text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}" design="None" press="._onCheckbook"/>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-StepTre}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-corr">
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"
                                         checked="{oDppStepThreeCom>/eMailCheck}" select="._onComEmailCheck"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-viaEmail}"/>
                        <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                       fieldType="Underlined" width="200px" value="{oDppStepThreeCom>/eMail}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"
                                         checked="{oDppStepThreeCom>/FaxCheck}" select="._onComFaxCheck"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-viaFax}"/>
                        <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                       fieldType="Underlined" width="200px" value="{oDppStepThreeCom>/Fax}"/>
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-incareof}"/>
                        <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                       fieldType="Underlined" width="330px" value="{oDppStepThreeCom>/Contract}"/>
                    </ute:Tag>
                </ute:Tag>
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-corr">
                    <ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"
                                         checked="{oDppStepThreeCom>/eAddrCheck}" select="._onComAddrCheck"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="{comp-i18n-billing>nrgBilling-dpp-viaMail}"/>
                        <!--<utmain:Dropdown class="nrgBilling-dlcc-chbrow-sel">
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option1" />
                            </utmain:DropdownItem>
                            <utmain:DropdownItem>
                                <ute:Tag elem="span" text="option2" />
                            </utmain:DropdownItem>
                        </utmain:Dropdown>-->
                    </ute:Tag>
                    <ute:Tag class="nrgBilling-dlcc-mail">
                        <ute:Tag class="nrgBilling-dlcc-mail-l">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-incareof}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="330px" value="{oDppStepThreeCom>/Contract}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <ute:Tag class="nrgBilling-dlcc-mail-row-column">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-stNum}"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="90px" value="{oDppStepThreeCom>/Address/HouseNo}"/>
                                </ute:Tag>
                                <ute:Tag class="nrgBilling-dlcc-mail-row-column">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-aptNo}"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="120px" value="{oDppStepThreeCom>/Address/UnitNo}"/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <ute:Tag class="nrgBilling-dlcc-mail-row-colCity">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-city}"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="232px" value="{oDppStepThreeCom>/Address/City}"/>
                                </ute:Tag>
                                <ute:Tag class="nrgBilling-dlcc-mail-row-colState">
                                    <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-state}"/>
                                    <ute:Textfield class="nrgBilling-dlcc-mail-valCol"
                                                   fieldType="Underlined" width="77px" value="{oDppStepThreeCom>/Address/State}"/>
                                </ute:Tag>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-country}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="340px" value="{oDppStepThreeCom>/Address/Country}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-addrTtl}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="307px" value="{oDppStepThreeCom>/Address/AddrLine}"/>
                            </ute:Tag>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlcc-mail-r">
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-stNAme}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="177px" value="{oDppStepThreeCom>/Address/Street}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-poBox}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="214px" value="{oDppStepThreeCom>/Address/PoBox}"/>
                            </ute:Tag>
                            <ute:Tag class="nrgBilling-dlcc-mail-row">
                                <utmain:Label class="nrgBilling-dlcc-mail-lbl" text="{comp-i18n-billing>nrgBilling-dpp-zip}"/>
                                <ute:Textfield class="nrgBilling-dlcc-mail-val"
                                               fieldType="Underlined" width="243px" value="{oDppStepThreeCom>/Address/ZipCode}"/>
                            </ute:Tag>
                        </ute:Tag>
                    </ute:Tag>
                    <!--<ute:Tag class="nrgBilling-dlcc-chbrow">
                        <utmain:Checkbox class="nrgBilling-dlcc-chbrow-chb"/>
                        <utmain:Label class="nrgBilling-dlcc-chbrow-lbl" text="Save New Address"/>
                    </ute:Tag>-->
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-btns">
                    <!--<utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnBack}"/>-->
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnSend}"
                                   press="._onDPPThirdStepSend"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnCncl}"
                                   press="._onDppConfCancelClick"/>
                </ute:Tag>
            </ute:Tag>
        </ute:Tag>
        <!--DPP Denied-->
        <ute:Tag class="nrgBilling-dPmtPln-lftArea"  visible="{oDppScrnControl>/DPPDenied}">
            <ute:Tag class="nrgBilling-dPmtPln-title" text="{comp-i18n-billing>nrgBilling-dpp-PageTitle}"/>
            <ute:Tag class="nrgBilling-dPmtPln-link">
                <utmain:Button text="{comp-i18n-billing>nrgBilling-dpp-LinkMain}" design="None" press="._onCheckbook"/>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-divider"/>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-top">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-top-l">
                    <ute:Tag class="nrgBilling-dltl-step" text="{comp-i18n-billing>nrgBilling-dpp-DppDenied}"/>
                </ute:Tag>
            </ute:Tag>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-ctnt-hdr">
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j1" text="{comp-i18n-billing>nrgBilling-dpp-Description}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j2" text="{comp-i18n-billing>nrgBilling-dpp-Amt}"/>
                    <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j3" text="{comp-i18n-billing>nrgBilling-dpp-DueDte}"/>
                </ute:Tag>
                <ute:Tag content="{oDppDeniedReason>/results}" class="nrgBilling-dPmtPln-lftArea-ctnt-tbl">
                    <ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-odd" visible="{parts:['oDppDeniedReason>DPPDenyed/iIndex'], formatter:'._isOdd'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j1" text="{oDppDeniedReason>DPPDenyed/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j2"
                                     text="{path: 'oDppDeniedReason>DPPDenyed/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j3" text="{parts:['oDppDeniedReason>DPPDenyed/DueDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                        <ute:Tag class="nrgBilling-dlct-evn" visible="{parts:['oDppDeniedReason>DPPDenyed/iIndex'], formatter:'._isEven'}">
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j1" text="{oDppDeniedReason>DPPDenyed/Description}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j2"
                                     text="{path: 'oDppDeniedReason>DPPDenyed/Amount', type: 'nrg.base.type.Price',
                                            formatOptions: { currencySymbol: '$' , currencyAlignment: 'LHS' }}"/>
                            <ute:Tag class="nrgBilling-dlc-cell nrgBilling-dlch-j3" text="{parts:['oDppDeniedReason>DPPDenyed/DueDate'], formatter:'._formatDate'}"/>
                        </ute:Tag>
                    </ute:Tag>
                </ute:Tag>
            </ute:Tag>
            <utmain:Infoline expanded="true" class="nrgBilling-dPmtPln-lftArea-ctnt-infoline">
                <utmain:headerContent>
                    <ute:Tag text="{comp-i18n-billing>nrgDppTool-ContactLogttl}" />
                 </utmain:headerContent>
                <utmain:content>
                    <ute:TextArea width="45rem" class="nrgBilling-dPmtPln-lftArea-ctnt-infoline-textArea" id="idnrgBilling-DPPDenCL"></u:TextArea>
                 </utmain:content>
            </utmain:Infoline>
            <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm">
                <ute:Tag class="nrgBilling-dPmtPln-lftArea-botm-btn-left">
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnOk}"
                                   press="._onDppDeniedOkClick"/>
                    <utmain:Button class="nrgBilling-dlbb-btn" text="{comp-i18n-billing>nrgBilling-dpp-BtnOver}" enabled="{oDppEligible>/DPPOverride}"
                                   press="._onDppOverrideClick"/>
                </ute:Tag>
            </ute:Tag>
        </ute:Tag>
        <!--Right Notification Area-->
        <ute:Tag class="nrgBilling-dPmtPln-rgtArea">
            <ute:Tag class="nrgBilling-dPmtPln-notify" text="{comp-i18n-billing>nrgBilling-dpp-NOTIFICATION}"/>
            <ute:Tag class="nrgBilling-dPmtPln-rgtArea-divider"/>
        </ute:Tag>
    </ute:Tag>
</mvc:View>

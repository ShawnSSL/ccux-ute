<core:FragmentDefinition
    xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="sap.ui.commons"
          xmlns:core="sap.ui.core"
          xmlns:u="ute.ui.commons"
          xmlns:um="ute.ui.main">
<u:Tag class="nrgBillDn-root">
    <u:Table width="auto"
             id="idnrgBillDn-Table"
             class="nrgBillDn-Table">
                    <u:columns>
                        <u:TableColumn width="8%" class="nrgBillDn-th">
                            <u:Tag text="Lotyp"/>
                        </u:TableColumn>
                        <u:TableColumn width="33%" class="nrgBillDn-th">
                            <u:Tag text="Proid"/>
                        </u:TableColumn>
                        <u:TableColumn width="14%" class="nrgBillDn-th">
                            <u:Tag text="Lockr"/>
                        </u:TableColumn>
                        <u:TableColumn width="11%" class="nrgBillDn-th">
                            <u:Tag text="Fdate"/>
                        </u:TableColumn>
                        <u:TableColumn width="13%" class="nrgBillDn-th">
                            <u:Tag text="Tdate"/>
                        </u:TableColumn>
                        <u:TableColumn width="10%" class="nrgBillDn-th">
                            <u:Tag text="Mansp"/>
                        </u:TableColumn>
                        <u:TableColumn width="11%" class="nrgBillDn-th">
                           <u:Tag text="Stext"/>
                        </u:TableColumn>
                    </u:columns>
            </u:Table>
        <!-- Place holder for templates to reuse for Bind Aggregation method. Below will be always Display:none -->
        <u:Tag visible="false">
             <u:Table width="60%">
                    <u:rows>
                    <u:TableRow id="idnrgBillDn-Row" class="nrgBillDn-Row" >
                        <u:Tag text="{comp-billing>Lotyp}"/>
                        <u:Tag text="{comp-billing>Proid}"/>
                        <u:Tag text="{comp-billing>Lockr}"/>
                        <u:Tag text="{path: 'comp-billing>Fdate', type: 'sap.ui.model.type.Date', formatOptions:{pattern: 'MM/dd/yyyy'} }"/>
                        <u:Tag text="{path: 'comp-billing>Tdate', type: 'sap.ui.model.type.Date', formatOptions:{pattern: 'MM/dd/yyyy'} }"/>
                        <u:Tag text="{comp-billing>Mansp}"/>
                        <u:Tag text="{comp-billing>Stext}"/>
                    </u:TableRow>
                </u:rows>
                </u:Table>
        </u:Tag>
        <!-- Place holder for templates to reuse for Bind Aggregation method. Below will be always Display:none -->
</u:Tag>
</core:FragmentDefinition>

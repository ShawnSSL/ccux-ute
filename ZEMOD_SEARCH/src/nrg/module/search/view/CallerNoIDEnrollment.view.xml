<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:utmain="ute.ui.main"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="nrg.module.search.view.CallerNoIDEnrollment">

    <ute:Tag class="nrgSearch-calrNoIDEnrol nrgAppMain-summaryContent nrgAppMain-summaryContent-full">
        <ute:content>
            <ute:Tag elem="h1"
                class="nrgSearch-calrNoIDEnrol-hdr"
                text="{comp-i18n-search>nrgSearchClrNid_EnrolTitle}">
            </ute:Tag>

            <ute:Tag elem="h2"
                class="nrgSearch-calrNoIDEnrol-title"
                text="{comp-i18n-search>nrgSearchClrNid_Enrol2ndTitle}">
            </ute:Tag>

            <ul class="nrgSearch-calrNoIDEnrol-list">
                <li class="nrgSearch-calrNoIDEnrol-listitem">
                    <utmain:Button
                        design="None"
                        text="{comp-i18n-search>nrgSearchClrNid_EnrollResident}"
                        press="onEnrollResident"
                        class="nrgCallerNoIDEnrollment-ListItemLink">
                    </utmain:Button>
                </li>
                <li class="nrgSearch-calrNoIDEnrol-listitem">
                    <utmain:Button
                        design="None"
                        text="{comp-i18n-search>nrgSearchClrNid_EnrollBus}"
                        press="onEnrollBusiness"
                        class="nrgCallerNoIDEnrollment-ListItemLink">
                    </utmain:Button>
                </li>
                <li class="nrgSearch-calrNoIDEnrol-listitem">
                    <utmain:Button
                        design="None"
                        text="{comp-i18n-search>nrgSearchClrNid_EnrollRHS}"
                        press="onEnrollRHS"
                        class="nrgCallerNoIDEnrollment-ListItemLink">
                    </utmain:Button>
                </li>
                <li class="nrgSearch-calrNoIDEnrol-listitem">
                    <utmain:Button
                        design="None"
                        text="{comp-i18n-search>nrgSearchClrNid_EnrollSecure}"
                        press="onEnrollHomeSecurity"
                        class="nrgCallerNoIDEnrollment-ListItemLink">
                    </utmain:Button>
                </li>
            </ul>
        </ute:content>
    </ute:Tag>
</mvc:View>

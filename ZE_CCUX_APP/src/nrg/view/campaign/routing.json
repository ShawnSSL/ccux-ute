{
    "sap.ui5": {
        "routing": {
            "routes": {
                "campaign": {
                    "pattern": "bp/{bpNum}/ca/{caNum}/campaign",
                    "target": [ "campaign.Overview", "dashboard.SummaryBP", "campaign.Tools" ]
                },
                "campaignchg": {
                    "pattern": "bp/{bpNum}/ca/{caNum}/campaign/cmpchg",
                    "target": [ "campaign.Change", "dashboard.SummaryBP", "campaign.Tools" ]
                },
                "campaignSS": {
                    "pattern": "bp/{bpNum}/ca/{caNum}/campaign/cmpss",
                    "target": [ "campaign.SalesScript", "dashboard.SummaryBP", "campaign.Tools" ]
                },
                "campaignoffers": {
                    "pattern": "bp/{bpNum}/ca/{caNum}/campaign/offers",
                    "target": [ "campaign.offers", "dashboard.SummaryBP", "campaign.Tools" ]
                }
            },
            "targets": {
                "campaign.General": {
                    "viewName": "campaign.General",
                    "controlId": "idAppGeneral"
                },
                "campaign.Overview": {
                    "viewName": "campaign.Overview",
                    "controlId": "idAppGeneral"
                },
                "campaign.History": {
                    "viewName": "campaign.History",
                    "controlId": "idAppGeneral"
                },
                "campaign.SalesScript": {
                    "viewName": "campaign.SalesScript",
                    "controlId": "idAppGeneral"
                },
                "campaign.offers": {
                    "viewName": "campaign.offers",
                    "controlId": "idAppGeneral"
                },
                "campaign.Change": {
                    "viewName": "campaign.Change",
                    "controlId": "idAppGeneral"
                },
                "campaign.Tools": {
                    "viewName": "campaign.Tools",
                    "controlId": "idAppTools"
                }
            }
        }
    }
}

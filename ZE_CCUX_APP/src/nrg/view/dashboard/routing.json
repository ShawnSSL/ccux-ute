{
    "sap.ui5": {
        "routing": {
            "routes": {
                "dashboard": {
                    "pattern": "",
                    "target": [ "dashboard.General", "dashboard.Summary", "dashboard.Tools" ]
                },

                "dashboard.Bp": {
                    "pattern": "bp/{bpNum}",
                    "target": [ "others.GeneralEmpty", "dashboard.SummaryBP", "others.ToolsEmpty" ]
                },

                "dashboard.BpCa": {
                    "pattern": "bp/{bpNum}/ca/{caNum}",
                    "target": [ "others.GeneralEmpty", "dashboard.SummaryBP", "others.ToolsEmpty" ]
                }
            },

            "targets": {
                "dashboard.General": {
                    "viewName": "dashboard.General",
                    "controlId": "idAppGeneral"
                },

                "dashboard.Summary": {
                    "viewName": "dashboard.Summary",
                    "controlId": "idAppSummary"
                },

                "dashboard.Tools": {
                    "viewName": "dashboard.Tools",
                    "controlId": "idAppTools"
                },

                "dashboard.SummaryBP": {
                    "viewName": "dashboard.SummaryBP",
                    "controlId": "idAppSummary"
                }
            }
        }
    }
}

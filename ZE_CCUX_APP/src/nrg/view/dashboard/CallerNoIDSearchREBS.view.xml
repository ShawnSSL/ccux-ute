<mvc:View
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ute="ute.ui.commons"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="nrg.view.dashboard.CallerNoIDSearchREBS">


    <ute:Tag class="nrgAppMain-generalContent nrgDashboard-calrNoIDSearch">
        <ute:content>
            <ute:Tag class="nrgDashboard-calrNoIDSearch-title"
                     text="{comp-i18n>nrgDashboardCallerNoIDSearchTitle}">
            </ute:Tag>
            <ute:Tag class="nrgDashboard-calrNoIDSearch-textfield">
                <ute:Textfield
                    class="nrgDashboard-calrNoIDSearch-textfield-input"
                    placeholder="Contract Account #"
                    change="testInputChange"
                    label="Title"
                    width="48%">
                </ute:Textfield>
                <ute:Textfield
                    class="nrgDashboard-calrNoIDSearch-textfield-input"
                    placeholder="SSN"
                    change="testInputChange"
                    label="Title"
                    width="48%">
                </ute:Textfield>
                <ute:Textfield
                    class="nrgDashboard-calrNoIDSearch-textfield-input"
                    placeholder="DL"
                    change="testInputChange"
                    label="Title"
                    width="48%">
                </ute:Textfield>
            </ute:Tag>
            <ute:Tag class="nrgDashboard-calrNoIDSearch-textfield">
                <ute:Textfield
                    class="nrgDashboard-calrNoIDSearch-textfield-input"
                    placeholder="Organization Name"
                    change="testInputChange"
                    label="Title"
                    width="48%">
                </ute:Textfield>
                <ute:Textfield
                    class="nrgDashboard-calrNoIDSearch-textfield-input"
                    placeholder="BP #"
                    change="testInputChange"
                    label="Title"
                    width="48%">
                </ute:Textfield>
                <ute:Textfield
                    class="nrgDashboard-calrNoIDSearch-textfield-input"
                    placeholder="Federal Tax ID"
                    change="testInputChange"
                    label="Title"
                    width="48%">
                </ute:Textfield>
                <ute:Textfield
                    class="nrgDashboard-calrNoIDSearch-textfield-input"
                    placeholder="SSN"
                    change="ESID"
                    label="Title"
                    width="48%">
                </ute:Textfield>
            </ute:Tag>
            <ute:Tag class="nrgCallerNoIDSearch-sbtn">
                <ute:Button
                    text="Search"
                    enabled="true">
                </ute:Button>
            </ute:Tag>
        </ute:content>
    </ute:Tag>
    <ute:Tag class="nrgDashboard-calrNoIDSearch nrgDashboard-calrNoIDSearch-transSpace">

    </ute:Tag>
    <ute:Tag class="nrgAppMain-generalContent nrgDashboard-calrNoIDSearch">
        <ute:Tag class="nrgDashboard-calrNoIDSearch-resultTitle"
                 text="{comp-i18n>nrgDashboardCallerNoIDSearchResultTitle}">
            </ute:Tag>
        <ute:Tag>
            <p>something</p>
        </ute:Tag>
    </ute:Tag>


</mvc:View>
